msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:96 /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:136
msgid "Vertices"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:90
msgid "Nearest Vertex"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:90
msgid "Uses source's nearest vertex."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:93
msgid "Nearest Edge Vertex"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:93
msgid "Uses source's nearest vertex of source's nearest edge."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:96
msgid "Nearest Face Vertex"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:96
msgid "Uses source's nearest vertex of source's nearest face."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:104 /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:141
msgid "Edges"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:100
msgid "Nearest Vertices"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:100
msgid "Uses source's edge which vertices are nearest from destination edge's vertices."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:102
msgid "Nearest Edge"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:103
msgid "Uses source's nearest edge (using edge's midpoints)."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:104
msgid "Nearest Face Edge"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:105
msgid "Uses source's nearest edge of source's nearest face (using edge's midpoints)."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:116 /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:150
msgid "Face Corners"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:107 /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:144
msgid "A face corner is not a real item by itself, it's some kind of split vertex attached to a specific face. Hence both vertex (location) and face (normal, ...) aspects are used to match them together."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:111
msgid "Nearest Corner and Best Matching Normal"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:111
msgid "Uses source's corner having the most similar *split* normal with destination one, from those sharing the nearest source's vertex."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:114
msgid "Nearest Corner and Best Matching Face Normal"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:114
msgid "Uses source's corner having the most similar *face* normal with destination one, from those sharing the nearest source's vertex."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:116
msgid "Nearest Corner of Nearest Face"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:117
msgid "Uses source's nearest corner of source's nearest face."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:123 /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:156
msgid "Faces"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:119
msgid "Nearest Face"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:120
msgid "Uses source's nearest face."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:123
msgid "Best Normal-Matching:"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:122
msgid "Uses source's face which normal is most similar with destination one."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:126
msgid "Interpolated Mappings"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:128
msgid "Those use several source items for each destination one, interpolating their data during the transfer."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:131
msgid "Nearest Edge Interpolated"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:132
msgid "Uses nearest point on nearest source's edge, interpolates data from both source edge's vertices."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:133 /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:147
msgid "Nearest Face Interpolated"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:134
msgid "Uses nearest point on nearest source's face, interpolates data from all that source face's vertices."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:136 /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:150 /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:156
msgid "Projected Face Interpolated"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:136
msgid "Uses point of face on source hit by projection of destination vertex along its own normal, interpolates data from all that source face's vertices."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:141
msgid "Projected Edge Interpolated"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:140
msgid "This is a sampling process. Several rays are cast from along the destination's edge (interpolating both edge's vertex normals), and if enough of them hit a source's edge, all hit source edges' data are interpolated into destination one."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:148
msgid "Uses nearest point of nearest source's face, interpolates data from all that source face's corners."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:150
msgid "Uses point of face on source hit by projection of destination corner along its own normal, interpolates data from all that source face's corners."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/scene_layout/object/editing/relations.rst:154
msgid "This is a sampling process. Several rays are cast from the whole destination's face (along its own normal), and if enough of them hit a source's face, all hit source faces' data are interpolated into destination one."
msgstr ""

