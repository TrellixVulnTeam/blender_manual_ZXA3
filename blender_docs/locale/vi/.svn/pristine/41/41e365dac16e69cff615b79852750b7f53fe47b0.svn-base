msgstr ""

#: C:/Users/Aaron/Documents/Projects/Blender_Foundation/blender_docs/manual/scene_layout/object/selecting.rst:243
msgid "Light Type"
msgstr ""

#: C:/Users/Aaron/Documents/Projects/Blender_Foundation/blender_docs/manual/scene_layout/object/selecting.rst:242
msgid "Select matching light types."
msgstr ""

#: C:/Users/Aaron/Documents/Projects/Blender_Foundation/blender_docs/manual/scene_layout/object/selecting.rst:248
msgid "Select Linked"
msgstr ""

