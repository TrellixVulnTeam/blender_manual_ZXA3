# Nút Màn Chắn Đôi Cạnh -- Double Edge Mask Node
msgid "Double Edge Mask Node"
msgstr "Nút Màn Chắn Đôi Cạnh"

msgid "Double Edge Mask Node."
msgstr "Nút Màn Chắn Đôi Cạnh."

msgid "The *Double Edge Mask* node creates a gradient between two masks."
msgstr "The *Màn Chắn Đôi Cạnh -- Double Edge Mask* node creates a gradient between two masks."

# Đầu Vào -- Inputs
msgid "Inputs"
msgstr "Đầu Vào"

# Màn Chắn Nội -- Inner Mask
msgid "Inner Mask"
msgstr "Màn Chắn Nội"

msgid "A mask representing the inside shape, which will be fully white."
msgstr "A mask representing the inside shape, which will be fully white."

# Màn Chắn Ngoại -- Outer Mask
msgid "Outer Mask"
msgstr "Màn Chắn Ngoại"

msgid "A mask representing the outside shape, which will fade from black at its edges to white at the *Inner Mask*."
msgstr "A mask representing the outside shape, which will fade from black at its edges to white at the *Màn Chắn Nội -- Inner Mask*."

# Tính Chất -- Properties
msgid "Properties"
msgstr "Tính Chất"

# Cạnh Bên Trong -- Inner Edge
msgid "Inner Edge"
msgstr "Cạnh Bên Trong"

# Toàn Bộ -- All
msgid "All"
msgstr "Toàn Bộ"

msgid "All shapes in the *Inner Mask* contribute to the gradient, even ones that do not touch the *Outer Mask* shape."
msgstr "All shapes in the *Màn Chắn Nội -- Inner Mask* contribute to the gradient, even ones that do not touch the *Màn Chắn Ngoại -- Outer Mask* shape."

# Duy Kề Cạnh -- Adjacent Only
msgid "Adjacent Only"
msgstr "Duy Kề Cạnh"

msgid "Only shapes in the *Inner Mask* that overlap with the *Outer Mask* contribute to the gradient."
msgstr "Only shapes in the *Màn Chắn Nội -- Inner Mask* that overlap with the *Màn Chắn Ngoại -- Outer Mask* contribute to the gradient."

msgid "All."
msgstr "All."

msgid "Adjacent Only."
msgstr "Adjacent Only."

# Cạnh Của Bộ Đệm -- Buffer Edge
msgid "Buffer Edge"
msgstr "Cạnh Của Bộ Đệm"

# Giữ Nằm Trong -- Keep In
msgid "Keep In"
msgstr "Giữ Nằm Trong"

msgid "Parts of the *Outer Mask* that touch the edge of the image are treated as if they stop at the edge."
msgstr "Parts of the *Màn Chắn Ngoại -- Outer Mask* that touch the edge of the image are treated as if they stop at the edge."

# Tràn Ra -- Bleed Out
msgid "Bleed Out"
msgstr "Tràn Ra"

msgid "Parts of the *Outer Mask* that touch the edge of the image are extended beyond the boundary of the image."
msgstr "Parts of the *Màn Chắn Ngoại -- Outer Mask* that touch the edge of the image are extended beyond the boundary of the image."

msgid "Keep In."
msgstr "Keep In."

msgid "Bleed Out."
msgstr "Bleed Out."

# Đầu Ra -- Outputs
msgid "Outputs"
msgstr "Đầu Ra"

# Màn Chắn -- Mask
msgid "Mask"
msgstr "Màn Chắn"

msgid "Standard mask output."
msgstr "Standard mask output."

