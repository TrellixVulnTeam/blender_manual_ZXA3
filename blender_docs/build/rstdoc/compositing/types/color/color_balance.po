# Nút Cân Bằng Màu -- Color Balance Node
msgid "Color Balance Node"
msgstr "Nút Cân Bằng Màu"

msgid "The Color Balance node can adjust the color and values of an image."
msgstr "Nút Cân Bằng Màu có thể điều chỉnh màu và giá trị của một hình ảnh."

msgid "Color Balance Node."
msgstr "Nút Chỉnh Sửa Màu."

# Đầu Vào -- Inputs
msgid "Inputs"
msgstr "Đầu Vào"

# Hệ Số -- Factor
msgid "Factor"
msgstr "Hệ Số"

msgid "Controls the amount of influence the node exerts on the output image."
msgstr "Điều chế lượng ảnh hưởng mà nút tác động đến hình ảnh xuất ra."

# Màu Sắc -- Color
msgid "Color"
msgstr "Màu Sắc"

msgid "Standard image input."
msgstr "Đầu vào hình ảnh tiêu chuẩn."

# Tính Chất -- Properties
msgid "Properties"
msgstr "Tính Chất"

msgid "Two different correction formulas could be selected."
msgstr "Có hai công thức hiệu chỉnh khác nhau có thể chọn."

# Lift/Gamma/Gain
msgid "Lift/Gamma/Gain"
msgstr "Nâng/Gamma/Gia Tăng"

# Nâng -- Lift
msgid "Lift"
msgstr "Nâng"

msgid "Increases the value of dark colors."
msgstr "Tăng giá trị của màu tối."

msgid "Gamma"
msgstr "Gamma"

msgid "Will adjust midtones."
msgstr "Sẽ điều chỉnh trung sắc (*midtones*) -- màu ở khoảng giữa điểm sáng nhất và tốt nhất."

# Gia Lượng -- Gain
msgid "Gain"
msgstr "Gia Lượng"

msgid "Adjusts highlights."
msgstr "Điều chỉnh các điểm nêu bật."

# Offset/Power/Slope (ASC-CDL)
msgid "Offset/Power/Slope (ASC-CDL)"
msgstr "Dịch Chuyển/Lũy Thừa/Dốc (ASC-CDL)"

# Dịch Chuyển -- Offset
msgid "Offset"
msgstr "Dịch Chuyển"

msgid "Summand. (Adjusts the overall brightness.)"
msgstr "Tổng Luận. (Điều chỉnh độ sáng tổng thể)."

# Cơ Sở -- Basis
msgid "Basis"
msgstr "Cơ Sở"

msgid "Additional offset, allows to specify a negative Offset value."
msgstr "Dịch chuyển bổ sung, cho phép để xác định một giá trị dịch chuyển âm."

# Năng Lượng/Sức Mạnh/Lũy Thừa -- Power
msgid "Power"
msgstr "Năng Lượng/Sức Mạnh/Lũy Thừa"

msgid "Over-all exponent. (Adjusts the midtones.)"
msgstr "Số mũ tổng quan. (Điều chỉnh trung sắc)."

# Độ Dốc -- Slope
msgid "Slope"
msgstr "Độ Dốc"

msgid "Multiplier. (Adjusts the highlights.)"
msgstr "Số Nhân. (Điều chỉnh các điểm nổi bật)."

# Đầu Ra -- Outputs
msgid "Outputs"
msgstr "Đầu Ra"

msgid "Standard output image."
msgstr "Hình ảnh đầu ra tiêu chuẩn."

# Cao Cấp -- Advanced
msgid "Advanced"
msgstr "Cao Cấp"

# The Offset/Power/Slope Formula
msgid "The Offset/Power/Slope Formula"
msgstr "Công Thức Dịch Chuyển/Lũy Thừa/Độ Dốc"

msgid "*out* = (*i* × *s* + *o*)\\ :sup:`p`"
msgstr "*ra -- out* = (*i* × *s* + *o*)\\ :sup:`p`"

msgid "where:"
msgstr "trong đó:"

# *ra -- out*: The color graded pixel code value.
msgid "*out*: The color graded pixel code value."
msgstr "*out*: Giá trị mã điểm ảnh cấp của màu sắc."

# *i*: The input pixel code value (0 to 1) (black to white).
msgid "*i*: The input pixel code value (0 to 1) (black to white)."
msgstr "*i*: Giá trị mã hóa điểm ảnh đầu vào (0 đến 1) (từ đen sang trắng)."

# *s*: Slope (any number 0 or greater, nominal value is 1.0).
msgid "*s*: Slope (any number 0 or greater, nominal value is 1.0)."
msgstr "*s*: Độ dốc (bất kỳ một số từ 0 trở lên, giá trị danh nghĩa là 1.0)."

# *o*: Offset (any number, the nominal value is 0).
msgid "*o*: Offset (any number, the nominal value is 0)."
msgstr "*o*: Dịch chuyển (bất kỳ số nào, giá trị danh nghĩa là 0)."

# *p*: Power (any number greater than 0, nominal value is 1.0).
msgid "*p*: Power (any number greater than 0, nominal value is 1.0)."
msgstr "*p*: (bất kỳ một số nào lớn hơn 0, giá trị danh nghĩa là 1.0)."

