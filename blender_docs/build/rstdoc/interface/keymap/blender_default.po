msgstr ""

msgid "Undo."
msgstr "Hoàn Tác."

msgid ":kbd:`Shift-Ctrl-Z`"
msgstr ""

msgid "Redo."
msgstr "Làm Lại."

msgid ":kbd:`Ctrl-Q`"
msgstr ""

msgid "Quit."
msgstr "Thoát."

msgid ":kbd:`F1`"
msgstr ""

msgid "Help *(context sensitive)*."
msgstr "Giúp Đỡ *(ngữ cảnh sensitive) -- (context sensitive)*."

msgid ":kbd:`F2`"
msgstr ""

msgid "Rename active item."
msgstr "Đổi Tên Mục Đang Hoạt Động."

msgid ":kbd:`F3`"
msgstr ""

msgid "Operator search."
msgstr "Tìm Kiếm Thao Tác/Toán Tử."

msgid ":kbd:`F4`"
msgstr ""

msgid "File context menu."
msgstr "Trình Đơn Ngữ Cảnh Tập Tin."

