# Đơn Giản Hóa -- Simplify
msgid "Simplify"
msgstr "Đơn Giản Hóa"

# Tham Chiếu -- Reference
msgid "Reference"
msgstr "Tham Chiếu"

msgid "Menu"
msgstr "Trình Đơn"

# :menuselection:`Render --> Simplify`
msgid ":menuselection:`Render --> Simplify`"
msgstr ":menuselection:`Cảnh (Scene) --> Đơn Giản Hóa (Simplify)`"

# Sắp Đặt Phổ Thông -- Common Settings
msgid "Common Settings"
msgstr "Sắp Đặt Phổ Thông"

# Mức Phân Hóa Tối Đa -- Max Subdivision
msgid "Max Subdivision"
msgstr "Mức Phân Hóa Tối Đa"

msgid "Maximum number of subdivision by the subdivision surface modifiers."
msgstr "Maximum number of subdivision by the subdivision surface modifiers."

# Số Hạt Con Cái -- Child Particles
msgid "Child Particles"
msgstr "Số Hạt Con Cái"

msgid "Show only a subset of all child hairs and particles."
msgstr "Show only a subset of all child hairs and particles."

# Giới Hạn Của Chất Liệu -- Texture Limit
msgid "Texture Limit"
msgstr "Giới Hạn Của Chất Liệu"

msgid "Automatically scales textures down so that they are no larger than the values chosen. This can help reduce computer memory resources when rendering large scenes with huge textures."
msgstr "Automatically scales textures down so that they are no larger than the values chosen. This can help reduce computer memory resources when rendering large scenes with huge textures."

# Va Đập của THTQXMT -- AO Bounces
msgid "AO Bounces"
msgstr "Va Đập của THTQXMT"

msgid "Replace global illumination with ambient occlusion after the specified number of bounces. This can reduce noise in interior scenes with little visual difference."
msgstr "Replace global illumination with ambient occlusion after the specified number of bounces. This can reduce noise in interior scenes with little visual difference."

# Cổng Nhìn -- Viewport
msgid "Viewport"
msgstr "Cổng Nhìn"

# Sử Dụng Khói với Độ Phân Giải Cao -- Use High-resolution Smoke
msgid "Use High-resolution Smoke"
msgstr "Sử Dụng Khói với Độ Phân Giải Cao"

msgid "Shows a :ref:`higher resolution <smoke-high-resolution>` version of :doc:`Smoke Simulations </physics/smoke/index>` in the viewport."
msgstr "Shows a :ref:`higher resolution <smoke-high-resolution>` version of :doc:`Smoke Simulations </physics/smoke/index>` in the viewport."

# Kết Xuất -- Render
msgid "Render"
msgstr "Kết Xuất"

msgid "See Common Settings above."
msgstr "See Common Settings above."

# Loại Trừ -- Culling
msgid "Culling"
msgstr "Loại Trừ"

# Loại Bỏ Trong Máy Quay Phim -- Use Camera Cull
msgid "Use Camera Cull"
msgstr "Loại Bỏ Trong Máy Quay Phim"

msgid "Automatically culls objects based on the camera frustum defined by the *Margin*."
msgstr "Automatically culls objects based on the camera frustum defined by the *Lề -- Margin*."

# Loại Bỏ Theo Khoảng Cách -- Use Distance Cull
msgid "Use Distance Cull"
msgstr "Loại Bỏ Theo Khoảng Cách"

msgid "Automatically culls objects based on their distance from the active camera. This is set via the *Distance* property."
msgstr "Automatically culls objects based on their distance from the active camera. This is set via the *Khoảng Cách -- Distance* property."

# Bút Chì Dầu -- Grease Pencil
msgid "Grease Pencil"
msgstr "Bút Chì Dầu"

# Duy Chơi Lại -- Playback Only
msgid "Playback Only"
msgstr "Duy Chơi Lại"

msgid "Activates the simplification process only during animation playback."
msgstr "Activates the simplification process only during animation playback."

# Bộ Điều Chỉnh -- Modifiers
msgid "Modifiers"
msgstr "Bộ Điều Chỉnh"

msgid "Disable Grease Pencil modifiers."
msgstr "Disable Grease Pencil modifiers."

msgid "ShaderFX"
msgstr "ShaderFX"

msgid "Disable Grease Pencil visual effects."
msgstr "Disable Grease Pencil visual effects."

msgid "Layer Blending"
msgstr "Layer Blending"

msgid "Disable the blending modes used in layers."
msgstr "Disable the blending modes used in layers."

msgid "Layer Tinting"
msgstr "Layer Tinting"

msgid "Disable layers tint overrides, only base material colors are displayed."
msgstr "Disable layers tint overrides, only base material colors are displayed."

# Không Phủ Kín -- Disable Fill
msgid "Disable Fill"
msgstr "Không Phủ Kín"

msgid "Disable the fill component in Grease Pencil materials."
msgstr "Disable the fill component in Grease Pencil materials."

# (Số) Đường Kẻ/Thẳng/Dòng -- Lines
msgid "Lines"
msgstr "(Số) Đường Kẻ/Thẳng/Dòng"

msgid "The strokes of materials that use a fill component are not displayed."
msgstr "The strokes of materials that use a fill component are not displayed."

