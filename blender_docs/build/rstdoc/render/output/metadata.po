# Siêu Dữ Liệu -- Metadata
msgid "Metadata"
msgstr "Siêu Dữ Liệu"

msgid "Metadata panel."
msgstr "Metadata panel."

msgid "The *Metadata* panel includes options for writing metadata into render output."
msgstr "The *Siêu Dữ Liệu -- Metadata* panel includes options for writing metadata into render output."

# Only some định dạng hình ảnh -- image formats support metadata: See :doc:`image formats </files/media/image_formats>`.
msgid "Only some image formats support metadata: See :doc:`image formats </files/media/image_formats>`."
msgstr "Only some image formats support metadata: See :doc:`image formats </data_system/files/media/image_formats>`."

# Siêu Dữ Liệu Sử Dụng -- Enabled Metadata
msgid "Enabled Metadata"
msgstr "Siêu Dữ Liệu Sử Dụng"

msgid "Stamping can include the following data:"
msgstr "Stamping can include the following data:"

# Ngày -- Date
msgid "Date"
msgstr "Ngày"

msgid "Includes the current date and time."
msgstr "Includes the current date and time."

# Thời Gian -- Time
msgid "Time"
msgstr "Thời Gian"

msgid "Includes the current scene time and render frame at ``HH:MM:SS.FF``."
msgstr "Includes the current scene time and render frame at ``HH:MM:SS.FF``."

# Thời Gian Kết Xuất -- Render Time
msgid "Render Time"
msgstr "Thời Gian Kết Xuất"

msgid "Includes the render time."
msgstr "Includes the render time."

# Khung Hình -- Frame
msgid "Frame"
msgstr "Khung Hình"

msgid "Includes the frame number."
msgstr "Includes the frame number."

# Phạm Vi Khung Hình -- Frame Range
msgid "Frame Range"
msgstr "Phạm Vi Khung Hình"

msgid "Includes the start and end frame numbers."
msgstr "Includes the start and end frame numbers."

# Bộ Nhớ -- Memory
msgid "Memory"
msgstr "Bộ Nhớ"

msgid "Includes the peak memory usage."
msgstr "Includes the peak memory usage."

# Hostname (Tên Máy) -- Hostname
msgid "Hostname"
msgstr "Hostname (Tên Máy)"

msgid "Includes the rendering machine's `hostname <https://en.wikipedia.org/wiki/Hostname>`__."
msgstr "Includes the rendering machine's `hostname (tên máy) -- hostname <https://en.wikipedia.org/wiki/Hostname>`__."

# Máy Quay Phim -- Camera
msgid "Camera"
msgstr "Máy Quay Phim"

msgid "Includes the name of the active camera."
msgstr "Includes the name of the active camera."

# Lăng Kính -- Lens
msgid "Lens"
msgstr "Lăng Kính"

msgid "Includes the name of the active camera's lens value."
msgstr "Includes the name of the active camera's lens value."

# Cảnh -- Scene
msgid "Scene"
msgstr "Cảnh"

msgid "Includes the name of the active scene."
msgstr "Includes the name of the active scene."

# Dấu Mốc/Bút Dạ -- Marker
msgid "Marker"
msgstr "Dấu Mốc/Bút Dạ"

msgid "Includes the name of the last marker."
msgstr "Includes the name of the last marker."

# Tên Tập Tin -- Filename
msgid "Filename"
msgstr "Tên Tập Tin"

msgid "Includes the filename of the blend-file."
msgstr "Includes the filename of the blend-file."

# Tên Dải -- Strip Name
msgid "Strip Name"
msgstr "Tên Dải"

msgid "Includes the name of the foreground sequence strip."
msgstr "Includes the name of the foreground sequence strip."

# Bộ Phối Hình -- Sequencer
msgid "Sequencer"
msgstr "Bộ Phối Hình"

# Siêu Dữ Liệu Của Dải -- Strip Metadata
msgid "Strip Metadata"
msgstr "Siêu Dữ Liệu Của Dải"

msgid "Use metadata from the strips in the Sequencer."
msgstr "Use metadata from the strips in the Sequencer."

# Ghi Chú -- Note
msgid "Note"
msgstr "Ghi Chú"

msgid "Includes a custom note."
msgstr "Includes a custom note."

msgid "It can be useful to use the *Note* field if you are setting up a render farm."
msgstr "It can be useful to use the *Ghi Chú -- Note* field if you are setting up a render farm."

msgid "Since you can script any information you like into it, such as an identifier for the render node or the job number."
msgstr "Since you can script any information you like into it, such as an identifier for the render node or the job number."

msgid "For details on stamping arbitrary values, see: `this page <https://blender.stackexchange.com/questions/26643>`__."
msgstr "For details on stamping arbitrary values, see: `this page <https://blender.stackexchange.com/questions/26643>`__."

# Đóng Dấu vào Hình Ảnh -- Burn into Image
msgid "Burn into Image"
msgstr "Đóng Dấu vào Hình Ảnh"

msgid "Add metadata as text to the render."
msgstr "Add metadata as text to the render."

# Kích Thước Phông -- Font Size
msgid "Font Size"
msgstr "Kích Thước Phông"

msgid "Set the size of the text."
msgstr "Set the size of the text."

# Màu Văn Bản -- Text Color
msgid "Text Color"
msgstr "Màu Văn Bản"

msgid "Set the color and alpha of the stamp text."
msgstr "Set the color and alpha of the stamp text."

# Màu/Ảnh/Nền -- Background
msgid "Background"
msgstr "Màu/Ảnh/Nền"

msgid "Set the color and alpha of the color behind the text."
msgstr "Set the color and alpha of the color behind the text."

# Bao Gồm Nhãn Hiệu -- Include Labels
msgid "Include Labels"
msgstr "Bao Gồm Nhãn Hiệu"

msgid "Displays the labels before the metadata text. For example, \"Camera\" in front of the camera name, etc."
msgstr "Displays the labels before the metadata text. For example, \"Camera\" in front of the camera name, etc."

