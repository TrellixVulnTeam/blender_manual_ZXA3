# Nút Đường Cong Vectơ -- Vector Curves Node
msgid "Vector Curves Node"
msgstr "Nút Đường Cong Vectơ"

# Nút Đường Cong Vectơ.ơ.
msgid "Vector Curves Node."
msgstr "Nút Đường Cong Vectơ."

msgid "The Vector Curves node maps an input vector components to a curve."
msgstr "Nút Đường Cong Vectơ ánh xạ các phần tử của một véctơ đầu vào thành một đường cong."

msgid "Use this curve node to slow things down or speed them up from the original scene."
msgstr "Sử dụng nút đường cong này để tăng hoặc giảm tốc độ của các vật thể trong cảnh gốc."

# Đầu Vào -- Inputs
msgid "Inputs"
msgstr "Đầu Vào"

msgid "In the shader context the node also has an additional Factor property."
msgstr "Trong bối cảnh chuyển sắc (tô bóng), nút này còn có thêm tính chất Hệ số (Factor) nữa."

# Hệ Số -- Factor
msgid "Factor"
msgstr "Hệ Số"

msgid "Controls the amount of influence the node exerts on the output vector."
msgstr "Điều khiển mức tác động của nút vào véctơ đầu ra."

# Véctơ -- Vector
msgid "Vector"
msgstr "Véctơ"

msgid "Standard vector input."
msgstr "Đầu vào vectơ tiêu chuẩn."

# Tính Chất -- Properties
msgid "Properties"
msgstr "Tính Chất"

# Kênh -- Channel
msgid "Channel"
msgstr "Kênh"

msgid "X, Y, Z"
msgstr "X, Y, Z"

# Đường Cong -- Curve
msgid "Curve"
msgstr "Đường Cong"

# For the curve controls see: :ref:`Thành Tố Đường Cong -- Curve widget <ui-curve-widget>`.
msgid "For the curve controls see: :ref:`Curve widget <ui-curve-widget>`."
msgstr "Để biết thêm về các điều khiển đường cong, xin xem thêm bài về :ref:`Thành Tố Đường Cong -- Curve widget <ui-curve-widget>`."

# Đầu Ra -- Outputs
msgid "Outputs"
msgstr "Đầu Ra"

msgid "Standard vector output."
msgstr "Đầu ra véctơ tiêu chuẩn."

