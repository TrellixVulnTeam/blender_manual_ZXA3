# Nút Chất Liệu Gạch -- Brick Texture Node
msgid "Brick Texture Node"
msgstr "Nút Chất Liệu Gạch"

msgid "Brick Texture Node."
msgstr "Brick Texture Node."

msgid "The *Brick Texture* is used to add a procedural texture producing bricks."
msgstr "The *Chất Liệu Gạch -- Brick Texture* is used to add a procedural texture producing bricks."

# Đầu Vào -- Inputs
msgid "Inputs"
msgstr "Đầu Vào"

msgid "Color 1/2 and Mortar"
msgstr "Color 1/2 and Mortar"

msgid "Color of the bricks and mortar."
msgstr "Color of the bricks and mortar."

# Tỷ Lệ -- Scale
msgid "Scale"
msgstr "Tỷ Lệ"

msgid "Overall texture scale."
msgstr "Tỷ Lệ Vật Thể Tổng Quan"

# Kích Thước Vữa -- Mortar Size
msgid "Mortar Size"
msgstr "Kích Thước Vữa"

msgid "The size of the filling between the bricks known as \"mortar\"; 0 means no mortar."
msgstr "The size of the filling between the bricks known as \"mortar\"; 0 means no mortar."

# Vữa Mịn -- Mortar Smooth
msgid "Mortar Smooth"
msgstr "Vữa Mịn"

msgid "Blurs/softens the edge between the mortar and the bricks. This can be useful with a texture and displacement textures."
msgstr "Blurs/softens the edge between the mortar and the bricks. This can be useful with a texture and displacement textures."

# Thiên Lệch -- Bias
msgid "Bias"
msgstr "Thiên Lệch"

msgid "The color variation between *Color 1/2*. Values of -1 and 1 only use one of the two colors; values in between mix the colors."
msgstr "The color variation between *Color 1/2*. Values of -1 and 1 only use one of the two colors; values in between mix the colors."

# Chiều Rộng Của Gạch -- Brick Width
msgid "Brick Width"
msgstr "Chiều Rộng Của Gạch"

msgid "The width of the bricks."
msgstr "The width of the bricks."

# Chiều Cao Của Hàng -- Row Height
msgid "Row Height"
msgstr "Chiều Cao Của Hàng"

msgid "The height of the brick rows."
msgstr "The height of the brick rows."

# Tính Chất -- Properties
msgid "Properties"
msgstr "Tính Chất"

# Dịch Chuyển -- Offset
msgid "Offset"
msgstr "Dịch Chuyển"

msgid "Determines the brick offset of the various rows."
msgstr "Determines the brick offset of the various rows."

# Tần Số -- Frequency
msgid "Frequency"
msgstr "Tần Số"

msgid "Determines the offset frequency. A value of 2 gives an even/uneven pattern of rows."
msgstr "Determines the offset frequency. A value of 2 gives an even/uneven pattern of rows."

# Nén Ép -- Squash
msgid "Squash"
msgstr "Nén Ép"

msgid "Amount of brick squashing."
msgstr "Amount of brick squashing."

msgid "Brick squashing frequency."
msgstr "Brick squashing frequency."

# Đầu Ra -- Outputs
msgid "Outputs"
msgstr "Đầu Ra"

# Màu Sắc -- Color
msgid "Color"
msgstr "Màu Sắc"

msgid "Texture color output."
msgstr "Đầu ra của màu chất liệu."

# Hệ Số -- Factor
msgid "Factor"
msgstr "Hệ Số"

msgid "Mortar mask (1 = mortar)."
msgstr "Mortar mask (1 = mortar)."

# Ví Dụ -- Examples
msgid "Examples"
msgstr "Ví Dụ"

msgid "Brick texture: Colors changed, Squash 0.62, Squash Frequency 3."
msgstr "Brick texture: Colors changed, Squash 0.62, Squash Frequency 3."

