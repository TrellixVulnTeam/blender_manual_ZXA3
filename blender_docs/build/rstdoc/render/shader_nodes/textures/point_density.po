# Nút Mật Độ Điểm -- Point Density Node
msgid "Point Density Node"
msgstr "Nút Mật Độ Điểm"

msgid "Point Density Node."
msgstr "Point Density Node."

msgid "The *Point Density* node is used to add volumetric points for each particle or vertex of another object."
msgstr "The *Mật Độ Điểm -- Point Density* node is used to add volumetric points for each particle or vertex of another object."

# Đầu Vào -- Inputs
msgid "Inputs"
msgstr "Đầu Vào"

# Véctơ -- Vector
msgid "Vector"
msgstr "Véctơ"

msgid "Texture coordinate to sample texture at; defaults to global position (*Position* output of *Geometry* node) if the socket is left unconnected."
msgstr "Texture coordinate to sample texture at; defaults to global position (*Vị Trí -- Position* output of *Hình Học -- Geometry* node) if the socket is left unconnected."

# Tính Chất -- Properties
msgid "Properties"
msgstr "Tính Chất"

msgid "Point Data"
msgstr "Point Data"

msgid "Where to get points from."
msgstr "Where to get points from."

# Hệ Thống Hạt -- Particle System
msgid "Particle System"
msgstr "Hệ Thống Hạt"

msgid "Use each particle position from the specified particle system."
msgstr "Use each particle position from the specified particle system."

# Điểm Đỉnh Của Vật Thể -- Object Vertices
msgid "Object Vertices"
msgstr "Điểm Đỉnh Của Vật Thể"

msgid "Use each vertex position from the specified object."
msgstr "Use each vertex position from the specified object."

# Vật Thể -- Object
msgid "Object"
msgstr "Vật Thể"

msgid "Which object's vertices or particle system will be used."
msgstr "Which object's vertices or particle system will be used."

msgid "Particle positions from this system will be used."
msgstr "Particle positions from this system will be used."

# Cách/Khoảng Trống/Không Gian -- Space
msgid "Space"
msgstr "Cách/Khoảng Trống/Không Gian"

msgid "The coordinate system for mapping points."
msgstr "The coordinate system for mapping points."

# Không Gian Thế Giới -- World Space
msgid "World Space"
msgstr "Không Gian Thế Giới"

msgid "Map each point exactly where the source particle/vertex is."
msgstr "Map each point exactly where the source particle/vertex is."

# Không Gian Vật Thể -- Object Space
msgid "Object Space"
msgstr "Không Gian Vật Thể"

msgid "Fit the points from the source particles/vertices inside the bounding box of the object with the point density texture."
msgstr "Fit the points from the source particles/vertices inside the bounding box of the object with the point density texture."

# Bán Kính -- Radius
msgid "Radius"
msgstr "Bán Kính"

msgid "Size of the points."
msgstr "Size of the points."

# Nội Suy -- Interpolation
msgid "Interpolation"
msgstr "Nội Suy"

msgid "Texel filtering type."
msgstr "Texel filtering type."

# Gần Nhất -- Closest
msgid "Closest"
msgstr "Gần Nhất"

msgid "No interpolation, use nearest texel. Produces blocky looking points."
msgstr "No interpolation, use nearest texel. Produces blocky looking points."

# Tuyến Tính -- Linear
msgid "Linear"
msgstr "Tuyến Tính"

msgid "Interpolate linearly between texels, producing soft, round points."
msgstr "Interpolate linearly between texels, producing soft, round points."

# Lập Phương -- Cubic
msgid "Cubic"
msgstr "Lập Phương"

msgid "Use cubic falloff, producing very soft points. Useful when points are very densely packed."
msgstr "Use cubic falloff, producing very soft points. Useful when points are very densely packed."

# Độ Phân Giải -- Resolution
msgid "Resolution"
msgstr "Độ Phân Giải"

msgid "The dimensions of the texture holding the point data."
msgstr "The dimensions of the texture holding the point data."

# Nguồn Màu Sắc -- Color Source
msgid "Color Source"
msgstr "Nguồn Màu Sắc"

msgid "Which attribute of the particle system or mesh is used to color the output."
msgstr "Which attribute of the particle system or mesh is used to color the output."

msgid "Particle Color Sources"
msgstr "Particle Color Sources"

# Tuổi Thọ Hạt -- Particle Age
msgid "Particle Age"
msgstr "Tuổi Thọ Hạt"

msgid "Lifetime mapped as (0.0 - 1.0) intensity."
msgstr "Lifetime mapped as (0.0 - 1.0) intensity."

# Tốc Độ Hạt -- Particle Speed
msgid "Particle Speed"
msgstr "Tốc Độ Hạt"

msgid "Particle speed (absolute magnitude of velocity) mapped as (0.0 - 1.0) intensity."
msgstr "Particle speed (absolute magnitude of velocity) mapped as (0.0 - 1.0) intensity."

# Vận Tốc Hạt -- Particle Velocity
msgid "Particle Velocity"
msgstr "Vận Tốc Hạt"

msgid "XYZ velocity mapped to RGB colors."
msgstr "XYZ velocity mapped to RGB colors."

msgid "Vertex Color Sources"
msgstr "Vertex Color Sources"

# Màu Điểm Đỉnh -- Vertex Color
msgid "Vertex Color"
msgstr "Màu Điểm Đỉnh"

msgid "Use a vertex color layer for coloring the point density texture."
msgstr "Use a vertex color layer for coloring the point density texture."

msgid "Vertex colors are defined per face corner. A single vertex can have as many different colors as faces it is part of. The actual color of the point density texture is averaged from all vertex corners."
msgstr "Vertex colors are defined per face corner. A single vertex can have as many different colors as faces it is part of. The actual color of the point density texture is averaged from all vertex corners."

# Trọng Lượng Điểm Đỉnh -- Vertex Weight
msgid "Vertex Weight"
msgstr "Trọng Lượng Điểm Đỉnh"

msgid "Use weights from a vertex group as intensity values."
msgstr "Use weights from a vertex group as intensity values."

# Pháp Tuyến Điểm Đỉnh -- Vertex Normals
msgid "Vertex Normals"
msgstr "Pháp Tuyến Điểm Đỉnh"

msgid "Use object-space vertex normals as RGB values."
msgstr "Use object-space vertex normals as RGB values."

# Đầu Ra -- Outputs
msgid "Outputs"
msgstr "Đầu Ra"

# Màu Sắc -- Color
msgid "Color"
msgstr "Màu Sắc"

msgid "Texture color output."
msgstr "Đầu ra của màu chất liệu."

# Tỷ Trọng -- Density
msgid "Density"
msgstr "Tỷ Trọng"

msgid "Density of volume."
msgstr "Density of volume."

# Ví Dụ -- Examples
msgid "Examples"
msgstr "Ví Dụ"

msgid "Domain object with Point Density texture using vertices from ball as points."
msgstr "Domain object with Point Density texture using vertices from ball as points."

