# BSDF Hoạt Họa -- Toon BSDF
msgid "Toon BSDF"
msgstr "BSDF Hoạt Họa"

msgid ":guilabel:`Cycles Only`"
msgstr ":guilabel:`Cycles Only`"

msgid "Toon BSDF."
msgstr "Toon BSDF."

msgid "The *Toon* :abbr:`BSDF (Bidirectional scattering distribution function)` is used to create *Diffuse* and *Glossy* materials with cartoon light effects."
msgstr "The *Toon* :abbr:`BSDF (Bidirectional scattering distribution function)` is used to create *Khuếch Tán -- Diffuse* and *Bóng Bẩy -- Glossy* materials with cartoon light effects."

# Đầu Vào -- Inputs
msgid "Inputs"
msgstr "Đầu Vào"

# Màu Sắc -- Color
msgid "Color"
msgstr "Màu Sắc"

msgid "Color of the surface, or physically speaking, the probability that light is reflected for each wavelength."
msgstr "Về màu sắc của bề mặt, hoặc về thể chất mà nói, xác suất ánh sáng được phản xạ cho mỗi bước sóng."

# Kích Thước -- Size
msgid "Size"
msgstr "Kích Thước"

msgid "Parameter between 0.0 and 1.0 that gives an angle of reflection between 0° and 90°."
msgstr "Parameter between 0.0 and 1.0 that gives an angle of reflection between 0° and 90°."

# Làm Mịn/Trơn Tru -- Smooth
msgid "Smooth"
msgstr "Làm Mịn/Trơn Tru"

msgid "This value specifies an angle over which a smooth transition from full to no reflection happens."
msgstr "This value specifies an angle over which a smooth transition from full to no reflection happens."

# Pháp Tuyến/B.Thg -- Normal
msgid "Normal"
msgstr "Pháp Tuyến/B.Thg"

msgid "Normal used for shading; if nothing is connected the default shading normal is used."
msgstr "Pháp tuyến sử dụng cho việc tô bóng; nếu không có gì kết nối thì pháp tuyến tô bóng mặc định sẽ được sử dụng."

# Tính Chất -- Properties
msgid "Properties"
msgstr "Tính Chất"

# Thành Phần -- Component
msgid "Component"
msgstr "Thành Phần"

# Khuếch Tán -- Diffuse
msgid "Diffuse"
msgstr "Khuếch Tán"

msgid "Use shading based on the Diffuse BSDF."
msgstr "Use shading based on the Diffuse BSDF."

# Bóng Bẩy -- Glossy
msgid "Glossy"
msgstr "Bóng Bẩy"

msgid "Use shading based on the Glossy BSDF for specular reflection."
msgstr "Use shading based on the Glossy BSDF for specular reflection."

# Đầu Ra -- Outputs
msgid "Outputs"
msgstr "Đầu Ra"

msgid "BSDF"
msgstr "BSDF"

msgid "Standard shader output."
msgstr "Đầu ra chuẩn của bộ tô bóng."

# Ví Dụ -- Examples
msgid "Examples"
msgstr "Ví Dụ"

msgid "Example of Toon Shader."
msgstr "Example of Toon Shader."

