# Nút Ánh Xạ Phạm Vi -- Map Range Node
msgid "Map Range Node"
msgstr "Nút Ánh Xạ Phạm Vi"

# Nút Ánh Xạ Phạm Vi. Vi.
msgid "Map Range Node."
msgstr "Nút Ánh Xạ Phạm Vi."

msgid "The *Map Range* node linearly remaps a value from a range to a target range."
msgstr "The *Ánh Xạ Phạm Vi -- Map Range* node linearly remaps a value from a range to a target range."

# Đầu Vào -- Inputs
msgid "Inputs"
msgstr "Đầu Vào"

# Giá Trị -- Value
msgid "Value"
msgstr "Giá Trị"

msgid "The input value to be remapped."
msgstr "The input value to be remapped."

# Từ Tối Thiểu -- From Min
msgid "From Min"
msgstr "Từ Tối Thiểu"

msgid "The lower bound of the range to remap from."
msgstr "The lower bound of the range to remap from."

# Từ Tối Đa -- From Max
msgid "From Max"
msgstr "Từ Tối Đa"

msgid "The higher bound of the range to remap from."
msgstr "The higher bound of the range to remap from."

# Đến Tối Thiểu -- To Min
msgid "To Min"
msgstr "Đến Tối Thiểu"

msgid "The lower bound of the target range."
msgstr "The lower bound of the target range."

# Đến Tối Đa -- To Max
msgid "To Max"
msgstr "Đến Tối Đa"

msgid "The higher bound of the target range."
msgstr "The higher bound of the target range."

# Tính Chất -- Properties
msgid "Properties"
msgstr "Tính Chất"

# Hạn Định -- Clamp
msgid "Clamp"
msgstr "Hạn Định"

msgid "If enabled, the output is clamped to the target range."
msgstr "If enabled, the output is clamped to the target range."

# Đầu Ra -- Outputs
msgid "Outputs"
msgstr "Đầu Ra"

# Kết Quả -- Result
msgid "Result"
msgstr "Kết Quả"

msgid "The input value after remapping."
msgstr "The input value after remapping."

# Ví Dụ -- Examples
msgid "Examples"
msgstr "Ví Dụ"

msgid "The *Noise Texture* node outputs a value in the range [0, 1]. We can use the *Map Range* node to remap this value into the range [-1, 1]."
msgstr "The *Chất Liệu Nhiễu -- Noise Texture* node outputs a value in the range [0, 1]. We can use the *Ánh Xạ Phạm Vi -- Map Range* node to remap this value into the range [-1, 1]."

msgid "Example of Map Range node."
msgstr "Example of Map Range node."

