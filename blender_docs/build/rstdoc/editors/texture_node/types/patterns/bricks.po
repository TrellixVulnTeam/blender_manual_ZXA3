msgid "Bricks Node"
msgstr "Bricks Node"

msgid "Bricks node."
msgstr "Bricks node."

msgid "The Bricks node creates a brick like pattern."
msgstr "The Bricks node creates a brick like pattern."

# Đầu Vào -- Inputs
msgid "Inputs"
msgstr "Đầu Vào"

msgid "Bricks 1, Bricks 2"
msgstr "Bricks 1, Bricks 2"

msgid "Sets the color range of the bricks. Brick colors are chosen randomly between these two colors."
msgstr "Sets the color range of the bricks. Brick colors are chosen randomly between these two colors."

# Vữa -- Mortar
msgid "Mortar"
msgstr "Vữa"

msgid "Sets the mortar color, in between the bricks."
msgstr "Sets the mortar color, in between the bricks."

# Độ Dày -- Thickness
msgid "Thickness"
msgstr "Độ Dày"

msgid "Sets the thickness of the mortar."
msgstr "Sets the thickness of the mortar."

# Thiên Lệch -- Bias
msgid "Bias"
msgstr "Thiên Lệch"

msgid "The bias of randomly chosen colors, between (-1 to 1). -1 Makes all bricks Color 1, and a value of 1 makes them all Color 2."
msgstr "The bias of randomly chosen colors, between (-1 to 1). -1 Makes all bricks Color 1, and a value of 1 makes them all Color 2."

# Chiều Rộng Của Gạch -- Brick Width
msgid "Brick Width"
msgstr "Chiều Rộng Của Gạch"

msgid "Sets the horizontal size of all the bricks."
msgstr "Sets the horizontal size of all the bricks."

# Chiều Cao Của Hàng -- Row Height
msgid "Row Height"
msgstr "Chiều Cao Của Hàng"

msgid "Sets the vertical size of all the bricks."
msgstr "Sets the vertical size of all the bricks."

# Tính Chất -- Properties
msgid "Properties"
msgstr "Tính Chất"

# Dịch Chuyển -- Offset
msgid "Offset"
msgstr "Dịch Chuyển"

msgid "The relative offset of the next row of bricks."
msgstr "The relative offset of the next row of bricks."

# Tần Số -- Frequency
msgid "Frequency"
msgstr "Tần Số"

msgid "Offset every N rows. The brick pattern offset repeats every N rows."
msgstr "Offset every N rows. The brick pattern offset repeats every N rows."

# Nén Ép -- Squash
msgid "Squash"
msgstr "Nén Ép"

msgid "Scales the bricks in every N rows by this amount."
msgstr "Scales the bricks in every N rows by this amount."

msgid "Squash every N rows."
msgstr "Squash every N rows."

# Đầu Ra -- Outputs
msgid "Outputs"
msgstr "Đầu Ra"

# Màu Sắc -- Color
msgid "Color"
msgstr "Màu Sắc"

msgid "Standard color output."
msgstr "Đầu ra màu sắc chuẩn."

