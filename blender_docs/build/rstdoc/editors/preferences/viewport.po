# Cổng Nhìn -- Viewport
msgid "Viewport"
msgstr "Cổng Nhìn"

msgid "Blender Preferences Viewport section."
msgstr "Blender Preferences Viewport section."

# Hiển Thị -- Display
msgid "Display"
msgstr "Hiển Thị"

# Thông Tin Vật Thể -- Object Info
msgid "Object Info"
msgstr "Thông Tin Vật Thể"

msgid "Display the active Object name and frame number at the bottom left of the 3D View."
msgstr "Display the active Object name and frame number at the bottom left of the 3D View."

# Tên Góc Nhìn -- View Name
msgid "View Name"
msgstr "Tên Góc Nhìn"

msgid "Display the name and type of the current view in the top left corner of the 3D View. For example: \"User Perspective\" or \"Top Orthographic\"."
msgstr "Display the name and type of the current view in the top left corner of the 3D View. For example: \"User Perspective\" or \"Top Orthographic\"."

# Tỷ Lệ Chơi Lại Khung Hình/Giây -- Playback FPS
msgid "Playback FPS"
msgstr "Tỷ Lệ Chơi Lại Khung Hình/Giây"

msgid "Show the frames per second screen refresh rate while an animation is played back. It appears in the viewport corner, displaying red if the frame rate set cannot be reached."
msgstr "Show the frames per second screen refresh rate while an animation is played back. It appears in the viewport corner, displaying red if the frame rate set cannot be reached."

# Gizmo Size
msgid "Gizmo Size"
msgstr "Kích Thước Gizmo"

msgid "Diameter of the gizmo."
msgstr "Diameter of the gizmo."

msgid "Look Dev Sphere Size"
msgstr "Look Dev Sphere Size"

msgid "Diameter of the LookDev sphere overlay."
msgstr "Diameter of the LookDev sphere overlay."

# Trục Cổng Nhìn 3D -- 3D Viewport Axis
msgid "3D Viewport Axis"
msgstr "Trục Cổng Nhìn 3D"

# Điều Hướng Tương Tác -- Interactive Navigation
msgid "Interactive Navigation"
msgstr "Điều Hướng Tương Tác"

msgid "Display the axis as an interactive gizmo."
msgstr "Display the axis as an interactive gizmo."

msgid "Click"
msgstr "Bấm Chuột"

msgid "Sets the viewport to display along this axis."
msgstr "Sets the viewport to display along this axis."

msgid "Drag"
msgstr "Lực Cản/Kéo Rê"

msgid "Orbit the view."
msgstr "Orbit the view."

# Trục Đơn Giản -- Simple Axis
msgid "Simple Axis"
msgstr "Trục Đơn Giản"

msgid "Display simple, less intrusive axis in the viewport."
msgstr "Display simple, less intrusive axis in the viewport."

msgid "Size"
msgstr "Kích Thước"

msgid "Size of the simple axis."
msgstr "Size of the simple axis."

msgid "Brightness"
msgstr "Độ Sáng Tối"

msgid "How vivid the colors of the simple axis are."
msgstr "How vivid the colors of the simple axis are."

# Tắt -- Off
msgid "Off"
msgstr "Tắt"

msgid "Disables the viewport axis."
msgstr "Disables the viewport axis."

# Chất Lượng -- Quality
msgid "Quality"
msgstr "Chất Lượng"

# Khử Răng Cưa Khung Chiếu -- Viewport Anti-aliasing
msgid "Viewport Anti-aliasing"
msgstr "Khử Răng Cưa Khung Chiếu"

msgid "Control the :term:`anti-aliasing` for higher quality rendering."
msgstr "Control the :term:`khử răng cưa -- anti-aliasing` for higher quality rendering."

# Lấy Đa Mẫu Vật -- Multisampling
msgid "Multisampling"
msgstr "Lấy Đa Mẫu Vật"

msgid "This enables :term:`multisampling` for higher quality rendering, at the expense of some performance."
msgstr "This enables :term:`lấy đa mẫu vật -- multisampling` for higher quality rendering, at the expense of some performance."

# Lấy Đa Mẫu Vật Cho Bút Chì Dầu -- Grease Pencil Multisampling
msgid "Grease Pencil Multisampling"
msgstr "Lấy Đa Mẫu Vật Cho Bút Chì Dầu"

msgid "Control the :term:`anti-aliasing` for higher quality Grease Pencil rendering."
msgstr "Control the :term:`khử răng cưa -- anti-aliasing` for higher quality Grease Pencil rendering."

# Mạch Lưới Mềm Mại Trong Chế Độ Biên Soạn -- Edit-Mode Smooth Wires
msgid "Edit-Mode Smooth Wires"
msgstr "Mạch Lưới Mềm Mại Trong Chế Độ Biên Soạn"

msgid "Display smooth wire in Edit Mode, without this wire will be rendered aliased."
msgstr "Display smooth wire in Edit Mode, without this wire will be rendered aliased."

msgid "Some users prefer to disable this for increased visibility, since edges don't blend into other shaded regions."
msgstr "Some users prefer to disable this for increased visibility, since edges don't blend into other shaded regions."

# Chất Liệu -- Textures
msgid "Textures"
msgstr "Chất Liệu"

# Kích Thước Giới Hạn -- Limit Size
msgid "Limit Size"
msgstr "Kích Thước Giới Hạn"

msgid "Limit the maximum resolution for pictures used in textured display to save memory. The limit options are specified in a square of pixels (e.g: the option 256 means a texture of 256×256 pixels). This is useful for game engineers, whereas the texture limit matches paging blocks of the textures in the target graphic card memory."
msgstr "Limit the maximum resolution for pictures used in textured display to save memory. The limit options are specified in a square of pixels (e.g: the option 256 means a texture of 256×256 pixels). This is useful for game engineers, whereas the texture limit matches paging blocks of the textures in the target graphic card memory."

# Lọc Dị Hướng -- Anisotropic Filtering
msgid "Anisotropic Filtering"
msgstr "Lọc Dị Hướng"

msgid "Sets the level of anisotropic filtering. This improves the quality of textures that are rendered at the cost of performance."
msgstr "Sets the level of anisotropic filtering. This improves the quality of textures that are rendered at the cost of performance."

# Cắt Alpha -- Clip Alpha
msgid "Clip Alpha"
msgstr "Cắt Alpha"

msgid "Clip alpha below this threshold in the 3D View. Note that, the default is set to a low value to prevent issues on some GPU's."
msgstr "Clip alpha below this threshold in the 3D View. Note that, the default is set to a low value to prevent issues on some GPU's."

# Phương Pháp Hiển Thị Hình Ảnh -- Image Display Method
msgid "Image Display Method"
msgstr "Phương Pháp Hiển Thị Hình Ảnh"

msgid "Method to render images; the following options are supported:"
msgstr "Method to render images; the following options are supported:"

# Tự Động -- Automatic
msgid "Automatic"
msgstr "Tự Động"

msgid "Automatically use *GLSL* which runs on the GPU for performance but falls back to the CPU for large images which might be slow when loaded with the GPU."
msgstr "Automatically use *GLSL* which runs on the GPU for performance but falls back to the CPU for large images which might be slow when loaded with the GPU."

# Chất Liệu 2D -- 2D Texture
msgid "2D Texture"
msgstr "Chất Liệu 2D"

msgid "Uses CPU for display transform and render images as a 2D texture."
msgstr "Uses CPU for display transform and render images as a 2D texture."

msgid "GLSL"
msgstr "GLSL"

msgid "Fastest method using GLSL for display transform and render images as a 2D texture."
msgstr "Fastest method using GLSL for display transform and render images as a 2D texture."

# Lựa Chọn -- Selection
msgid "Selection"
msgstr "Lựa Chọn"

# Chọn Lọc theo Chiều Sâu OpenGL -- OpenGL Depth Picking
msgid "OpenGL Depth Picking"
msgstr "Chọn Lọc theo Chiều Sâu OpenGL"

msgid "This option uses an alternative method of picking which uses depth information to select the front-most elements. It is only used for selecting with the cursor (not box select, lasso, circle select, etc.)."
msgstr "This option uses an alternative method of picking which uses depth information to select the front-most elements. It is only used for selecting with the cursor (not box select, lasso, circle select, etc.)."

msgid "Performance varies depending on your OpenGL hardware and drivers."
msgstr "Performance varies depending on your OpenGL hardware and drivers."

