# Bảng Hiển Thị -- Display Panel
msgid "Display Panel"
msgstr "Bảng Hiển Thị"

msgid "You can set the editors display options in this panel."
msgstr "You can set the editors display options in this panel."

msgid "Display panel: With both an image and UVs selected."
msgstr "Display panel: With both an image and UVs selected."

# Tỷ Lệ Khung Hình -- Aspect Ratio
msgid "Aspect Ratio"
msgstr "Tỷ Lệ Khung Hình"

msgid "Display Aspect for this image. Does not affect rendering."
msgstr "Display Aspect for this image. Does not affect rendering."

# Nhắc Lại Hình Ảnh -- Repeat Image
msgid "Repeat Image"
msgstr "Nhắc Lại Hình Ảnh"

msgid "Duplicate the image until it is repeated to fill the main view."
msgstr "Duplicate the image until it is repeated to fill the main view."

# Tọa Độ Của Điểm Ảnh -- Pixel Coordinates
msgid "Pixel Coordinates"
msgstr "Tọa Độ Của Điểm Ảnh"

msgid "Display UV coordinates in pixels rather than from 0.0 to 1.0"
msgstr "Hiển thị tọa độ UV trong đơn vị điểm ảnh, thay vì là từ 0.0 đến 1.0"

# Lớp Vẽ Lồng -- Overlays
msgid "Overlays"
msgstr "Lớp Vẽ Lồng"

# Hình Thức Biểu Hiện -- Display As
msgid "Display As"
msgstr "Hình Thức Biểu Hiện"

# Nét Viền -- Outline
msgid "Outline"
msgstr "Nét Viền"

msgid "Display white edges with black outline."
msgstr "Display white edges with black outline."

# Đường Vạch Đứt Đoạn -- Dash
msgid "Dash"
msgstr "Đường Vạch Đứt Đoạn"

msgid "Display dashed black-white edges."
msgstr "Display dashed black-white edges."

# Màu Đen -- Black
msgid "Black"
msgstr "Màu Đen"

msgid "Display black edges."
msgstr "Display black edges."

# Màu Trắng -- White
msgid "White"
msgstr "Màu Trắng"

msgid "Display white edges."
msgstr "Display white edges."

# Bề Mặt -- Faces
msgid "Faces"
msgstr "Bề Mặt"

msgid "Display faces over the image."
msgstr "Display faces over the image."

# Làm Mịn/Trơn Tru -- Smooth
msgid "Smooth"
msgstr "Làm Mịn/Trơn Tru"

msgid "Makes edges appear anti-aliased."
msgstr "Makes edges appear anti-aliased."

# Đã Bị Sửa Đổi -- Modified
msgid "Modified"
msgstr "Đã Bị Sửa Đổi"

msgid "Show results of modifiers in the UV display."
msgstr "Show results of modifiers in the UV display."

# Co Giãn -- Stretching
msgid "Stretching"
msgstr "Co Giãn"

msgid "Shows how much of a difference there is between UV coordinates and 3D coordinates. Blue means low distortion, while Red means high distortion. Choose to display the distortion of *Angles* or the *Area*."
msgstr "Shows how much of a difference there is between UV coordinates and 3D coordinates. Blue means low distortion, while Red means high distortion. Choose to display the distortion of *Angles* or the *Diện Tích -- Area*."

