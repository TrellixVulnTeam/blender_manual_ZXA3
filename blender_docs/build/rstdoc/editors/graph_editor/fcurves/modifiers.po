# Bộ Điều Chỉnh Đường Cong-F -- F-Curve Modifiers
msgid "F-Curve Modifiers"
msgstr "Bộ Điều Chỉnh Đường Cong-F"

# Tham Chiếu -- Reference
msgid "Reference"
msgstr "Tham Chiếu"

msgid "Panel"
msgstr "Bảng"

# :menuselection:`Sidebar region --> Modifiers --> Modifiers`
msgid ":menuselection:`Sidebar region --> Modifiers --> Modifiers`"
msgstr ":menuselection:`Vùng Tính Chất (Properties region) --> Bộ Điều Chỉnh (Modifiers) --> Bộ Điều Chỉnh (Modifiers)`"

msgid "F-Curve modifiers are similar to object modifiers, in that they add non-destructive effects, that can be adjusted at any time, and layered to create more complex effects."
msgstr "F-Curve modifiers are similar to object modifiers, in that they add non-destructive effects, that can be adjusted at any time, and layered to create more complex effects."

# Thêm một Bộ Điều Chỉnh -- Adding a Modifier
msgid "Adding a Modifier"
msgstr "Thêm một Bộ Điều Chỉnh"

# Bảng các bộ điều chỉnh. chỉnh.
msgid "Modifiers panel."
msgstr "Bảng các bộ điều chỉnh."

msgid "The F-curve modifier panel is located in the Sidebar region. Select a curve by selecting one of its curve points, or by selecting the channel list. Click on the *Add Modifier* menu to select a modifier."
msgstr "The F-curve modifier panel is located in the Sidebar region. Select a curve by selecting one of its curve points, or by selecting the channel list. Click on the *Thêm Bộ Điều Chỉnh -- Add Modifier* menu to select a modifier."

# Thể Loại của Bộ Điều Chỉnh -- Types of Modifiers
msgid "Types of Modifiers"
msgstr "Thể Loại của Bộ Điều Chỉnh"

# Hàm Sinh -- Generator
msgid "Generator"
msgstr "Hàm Sinh"

msgid "Generator creates a Factorized or Expanded Polynomial function. These are basic mathematical formulas that represent lines, parabolas, and other more complex curves, depending on the values used."
msgstr "Generator creates a Factorized or Expanded Polynomial function. These are basic mathematical formulas that represent lines, parabolas, and other more complex curves, depending on the values used."

# Bổ Sung/Cộng -- Additive
msgid "Additive"
msgstr "Bổ Sung/Cộng"

msgid "This option causes the modifier to be added to the curve, instead of replacing it by default."
msgstr "This option causes the modifier to be added to the curve, instead of replacing it by default."

# Bậc Phương Trình -- Polynomial Order
msgid "Polynomial Order"
msgstr "Bậc Phương Trình"

# Specify the order of the polynomial, or the highest power of ``X`` for this polynomial. (Number of coefficients: 1.)
msgid "Specify the order of the polynomial, or the highest power of ``X`` for this polynomial. (Number of coefficients: 1.)"
msgstr "Specify the order of the polynomial, or the highest power of ``X`` for this polynomial. (number of coefficients: 1)."

msgid "Change the Coefficient values to change the shape of the curve."
msgstr "Change the Coefficient values to change the shape of the curve."

msgid "`The Wikipedia Page <https://en.wikipedia.org/wiki/Polynomial>`__ for more information on polynomials."
msgstr "`The Wikipedia Page <https://en.wikipedia.org/wiki/Polynomial>`__ for more information on polynomials."

# Hàm Tích Hợp -- Built-in Function
msgid "Built-in Function"
msgstr "Hàm Tích Hợp"

msgid "These are additional formulas, each with the same options to control their shape. Consult mathematics reference for more detailed information on each function:"
msgstr "These are additional formulas, each with the same options to control their shape. Consult mathematics reference for more detailed information on each function:"

msgid "Sine"
msgstr "Sin"

msgid "Cosine"
msgstr "Cos"

msgid "Tangent"
msgstr "Tiếp Tuyến/Tang"

msgid "Square Root"
msgstr "Canh"

msgid "Natural Logarithm"
msgstr "Lôgarit Tự Nhiên"

msgid "Normalized Sine (``sin(x)/x``)"
msgstr "Normalized Sine (``sin(x)/x``)"

# Biên Độ -- Amplitude
msgid "Amplitude"
msgstr "Biên Độ"

msgid "Adjusts the Y scaling."
msgstr "Adjusts the Y scaling."

# Hệ Số Khuếch Đại Pha -- Phase Multiplier
msgid "Phase Multiplier"
msgstr "Hệ Số Khuếch Đại Pha"

msgid "Adjusts the X scaling."
msgstr "Adjusts the X scaling."

# Dịch Chuyển Pha Sóng -- Phase Offset
msgid "Phase Offset"
msgstr "Dịch Chuyển Pha Sóng"

msgid "Adjusts the X offset."
msgstr "Adjusts the X offset."

# Giá Trị Dịch Chuyển -- Value Offset
msgid "Value Offset"
msgstr "Giá Trị Dịch Chuyển"

msgid "Adjusts the Y offset."
msgstr "Adjusts the Y offset."

# Vỏ Bao -- Envelope
msgid "Envelope"
msgstr "Vỏ Bao"

msgid "Allows you to adjust the overall shape of a curve with control points."
msgstr "Allows you to adjust the overall shape of a curve with control points."

# Giá Trị Tham Chiếu -- Reference Value
msgid "Reference Value"
msgstr "Giá Trị Tham Chiếu"

msgid "Set the Y value the envelope is centered around."
msgstr "Set the Y value the envelope is centered around."

# Tối Thiểu -- Min
msgid "Min"
msgstr "Tối Thiểu"

msgid "Lower distance from Reference Value for ``1:1`` default influence."
msgstr "Lower distance from Reference Value for ``1:1`` default influence."

# Tối Đa -- Max
msgid "Max"
msgstr "Tối Đa"

msgid "Upper distance from Reference Value for ``1:1`` default influence."
msgstr "Upper distance from Reference Value for ``1:1`` default influence."

# Cộng Thêm Điểm -- Add Point
msgid "Add Point"
msgstr "Cộng Thêm Điểm"

msgid "Add a set of control points. They will be created at the current frame."
msgstr "Add a set of control points. They will be created at the current frame."

# Điểm/Chấm -- Point
msgid "Point"
msgstr "Điểm/Chấm"

# Khung Hình -- Frame
msgid "Frame"
msgstr "Khung Hình"

msgid "Set the frame number for the control point."
msgstr "Set the frame number for the control point."

msgid "Specifies the lower control point's position."
msgstr "Specifies the lower control point's position."

msgid "Specifies the upper control point's position."
msgstr "Specifies the upper control point's position."

# Cycles
msgid "Cycles"
msgstr "Tuần Hoàn"

msgid "Cycles allows you add cyclic motion to a curve that has two or more control points. The options can be set for before and after the curve."
msgstr "Cycles allows you add cyclic motion to a curve that has two or more control points. The options can be set for before and after the curve."

# Chế Độ Nhắc Lại -- Cycle Mode
msgid "Cycle Mode"
msgstr "Chế Độ Nhắc Lại"

# Nhắc Lại Chuyển Động -- Repeat Motion
msgid "Repeat Motion"
msgstr "Nhắc Lại Chuyển Động"

msgid "Repeats the curve data, while maintaining their values each cycle."
msgstr "Repeats the curve data, while maintaining their values each cycle."

# Nhắc Lại với Dịch Chuyển -- Repeat with Offset
msgid "Repeat with Offset"
msgstr "Nhắc Lại với Dịch Chuyển"

msgid "Repeats the curve data, but offsets the value of the first point to the value of the last point each cycle."
msgstr "Repeats the curve data, but offsets the value of the first point to the value of the last point each cycle."

# Nhắc Lại Đối Xứng Hóa -- Repeat Mirrored
msgid "Repeat Mirrored"
msgstr "Nhắc Lại Đối Xứng Hóa"

msgid "Each cycle the curve data is flipped across the X axis."
msgstr "Each cycle the curve data is flipped across the X axis."

# Tuần Hoàn Trước, Sau -- Before, After Cycles
msgid "Before, After Cycles"
msgstr "Tuần Hoàn Trước, Sau"

msgid "Set the number of times to cycle the data. A value of 0 cycles the data infinitely."
msgstr "Set the number of times to cycle the data. A value of 0 cycles the data infinitely."

msgid "Trivially Cyclic Curves"
msgstr "Trivially Cyclic Curves"

msgid "When the *Cycle Mode* for both ends is set to either *Repeat Motion* or *Repeat with Offset*, and no other options of the modifier are changed from their defaults, it defines a simple infinite cycle."
msgstr "When the *Chế Độ Nhắc Lại -- Cycle Mode* for both ends is set to either *Nhắc Lại Chuyển Động -- Repeat Motion* or *Nhắc Lại với Dịch Chuyển -- Repeat with Offset*, and no other options of the modifier are changed from their defaults, it defines a simple infinite cycle."

msgid "This special case receives some additional support from other areas of Blender:"
msgstr "This special case receives some additional support from other areas of Blender:"

msgid "Automatic Bézier handle placement is aware of the cycle and adjusts to achieve a smooth transition."
msgstr "Automatic Bézier handle placement is aware of the cycle and adjusts to achieve a smooth transition."

msgid "The :ref:`Cycle-Aware Keying <timeline-keying>` option can be enabled to take the cycle into account when inserting new keyframes."
msgstr "The :ref:`Khóa Hóa Cảnh Giác Về Sự Tuần Hoàn -- Cycle-Aware Keying <timeline-keying>` option can be enabled to take the cycle into account when inserting new keyframes."

# Nhiễu -- Noise
msgid "Noise"
msgstr "Nhiễu"

msgid "Modifies the curve with a noise formula. This is useful for creating subtle or extreme randomness to animated movements, like camera shake."
msgstr "Modifies the curve with a noise formula. This is useful for creating subtle or extreme randomness to animated movements, like camera shake."

# Kiểu Hòa Trộn -- Blend Type
msgid "Blend Type"
msgstr "Kiểu Hòa Trộn"

# Thay Thế -- Replace
msgid "Replace"
msgstr "Thay Thế"

msgid "Adds a -0.5 to 0.5 range noise function to the curve."
msgstr "Adds a -0.5 to 0.5 range noise function to the curve."

# Cộng Thêm -- Add
msgid "Add"
msgstr "Cộng Thêm"

msgid "Adds a 0 to 1 range noise function to the curve."
msgstr "Adds a 0 to 1 range noise function to the curve."

# Trừ -- Subtract
msgid "Subtract"
msgstr "Trừ"

msgid "Subtracts a 0 to 1 range noise function to the curve."
msgstr "Subtracts a 0 to 1 range noise function to the curve."

# Nhân -- Multiply
msgid "Multiply"
msgstr "Nhân"

msgid "Multiplies a 0 to 1 range noise function to the curve."
msgstr "Multiplies a 0 to 1 range noise function to the curve."

# Tỷ Lệ -- Scale
msgid "Scale"
msgstr "Tỷ Lệ"

msgid "Adjust the overall size of the noise. Values further from 0 give less frequent noise."
msgstr "Adjust the overall size of the noise. Values further from 0 give less frequent noise."

# Độ Đậm/Sức Mạnh/Cường Độ -- Strength
msgid "Strength"
msgstr "Độ Đậm/Sức Mạnh/Cường Độ"

msgid "Adjusts the Y scaling of the noise function."
msgstr "Adjusts the Y scaling of the noise function."

# Dịch Chuyển -- Offset
msgid "Offset"
msgstr "Dịch Chuyển"

msgid "Offsets the noise in time."
msgstr "Offsets the noise in time."

# Pha Sóng -- Phase
msgid "Phase"
msgstr "Pha Sóng"

msgid "Adjusts the random seed of the noise."
msgstr "Adjusts the random seed of the noise."

# Chiều/Độ Sâu -- Depth
msgid "Depth"
msgstr "Chiều/Độ Sâu"

msgid "Adjusts how detailed the noise function is."
msgstr "Adjusts how detailed the noise function is."

# Giới Hạn -- Limits
msgid "Limits"
msgstr "Giới Hạn"

msgid "Limit curve values to specified X and Y ranges."
msgstr "Limit curve values to specified X and Y ranges."

# Tối Thiểu, Tối Đa X -- Minimum, Maximum X
msgid "Minimum, Maximum X"
msgstr "Tối Thiểu, Tối Đa X"

msgid "Cuts a curve off at these frames ranges, and sets their minimum value at those points."
msgstr "Cuts a curve off at these frames ranges, and sets their minimum value at those points."

# Tối Thiểu, Tối Đa Y -- Minimum, Maximum Y
msgid "Minimum, Maximum Y"
msgstr "Tối Thiểu, Tối Đa Y"

msgid "Truncates the curve values to a range."
msgstr "Truncates the curve values to a range."

# Nội Suy Phân Bậc -- Stepped Interpolation
msgid "Stepped Interpolation"
msgstr "Nội Suy Phân Bậc"

msgid "Gives the curve a stepped appearance by rounding values down within a certain range of frames."
msgstr "Gives the curve a stepped appearance by rounding values down within a certain range of frames."

# Kích Thước Bậc Thang -- Step Size
msgid "Step Size"
msgstr "Kích Thước Bậc Thang"

msgid "Specify the number of frames to hold each frame."
msgstr "Specify the number of frames to hold each frame."

msgid "Reference number of frames before frames get held. Use to get hold for (1-3) vs (5-7) holding patterns."
msgstr "Reference number of frames before frames get held. Use to get hold for (1-3) vs (5-7) holding patterns."

# Dùng Khung Hình Đầu Tiên -- Use Start Frame
msgid "Use Start Frame"
msgstr "Dùng Khung Hình Đầu Tiên"

# Restrict modifier to only act before its "end" frame.
msgid "Restrict modifier to only act before its \"end\" frame."
msgstr "Hạn chế bộ điều chỉnh để chỉ hoạt động trước khung hình \"cuối\" của nó."

# Dùng Khung Hình Cuối Cùng -- Use End Frame
msgid "Use End Frame"
msgstr "Dùng Khung Hình Cuối Cùng"

# Restrict modifier to only act after its "start" frame.
msgid "Restrict modifier to only act after its \"start\" frame."
msgstr "Hạn chế bộ điều chỉnh để chỉ hoạt động sau khung hình \"đầu\" của nó."

