# Gizmos Của Cổng Nhìn -- Viewport Gizmos
msgid "Viewport Gizmos"
msgstr "Gizmos Của Cổng Nhìn"

msgid "The way how gizmos are displayed in the 3D View can be changed in the Viewport Gizmos pop-over. There is a switch to turn off all gizmos for the 3D View."
msgstr "The way how gizmos are displayed in the 3D View can be changed in the Viewport Gizmos pop-over. There is a switch to turn off all gizmos for the 3D View."

# Điều Hành -- Navigate
msgid "Navigate"
msgstr "Điều Hành"

msgid "Enable/disable the navigation gizmo."
msgstr "Enable/disable the navigation gizmo."

# Công Cụ Đang Hoạt Động -- Active Tool
msgid "Active Tool"
msgstr "Công Cụ Đang Hoạt Động"

msgid "Enable/disable the gizmo of the active tool."
msgstr "Enable/disable the gizmo of the active tool."

# Vật Thể Đang Hoạt Động -- Active Object
msgid "Active Object"
msgstr "Vật Thể Đang Hoạt Động"

msgid "Enable/disable the gizmo for the active object."
msgstr "Enable/disable the gizmo for the active object."

# Gizmo Của Vật Thể -- Object Gizmos
msgid "Object Gizmos"
msgstr "Gizmo Của Vật Thể"

# Định Hướng -- Orientation
msgid "Orientation"
msgstr "Định Hướng"

msgid "The orientation to use for the gizmo. The orientations can be configured in the viewport orientation :doc:`Orientations menu </editors/3dview/controls/orientation>`."
msgstr "The orientation to use for the gizmo. The orientations can be configured in the viewport orientation :doc:`Orientations menu </editors/3dview/controls/orientation>`."

# Di Chuyển -- Move
msgid "Move"
msgstr "Di Chuyển"

msgid "Show the gizmo to control the location."
msgstr "Show the gizmo to control the location."

# Xoay Chiều -- Rotate
msgid "Rotate"
msgstr "Xoay Chiều"

msgid "Show the gizmo to control the rotation."
msgstr "Show the gizmo to control the rotation."

# Tỷ Lệ -- Scale
msgid "Scale"
msgstr "Tỷ Lệ"

msgid "Show the gizmo to control the scaling."
msgstr "Show the gizmo to control the scaling."

# Phần Tử Trống/Rỗng -- Empty
msgid "Empty"
msgstr "Phần Tử Trống/Rỗng"

msgid "Gizmo settings for empties."
msgstr "Gizmo settings for empties."

# Hình Ảnh -- Image
msgid "Image"
msgstr "Hình Ảnh"

msgid "Show the gizmo to adjust the image size and position of empties."
msgstr "Show the gizmo to adjust the image size and position of empties."

# Trường Lực -- Force Field
msgid "Force Field"
msgstr "Trường Lực"

msgid "Show the gizmo to adjust the force field."
msgstr "Show the gizmo to adjust the force field."

# Nguồn/Ánh Sáng -- Light
msgid "Light"
msgstr "Nguồn/Ánh Sáng"

msgid "Gizmo settings for lights."
msgstr "Gizmo settings for lights."

# Kích Thước -- Size
msgid "Size"
msgstr "Kích Thước"

msgid "Show the gizmo to adjust the size of lights."
msgstr "Show the gizmo to adjust the size of lights."

# Nhìn Vào -- Look At
msgid "Look At"
msgstr "Nhìn Vào"

msgid "Show the gizmo to adjust the direction of the light."
msgstr "Show the gizmo to adjust the direction of the light."

# Máy Quay Phim -- Camera
msgid "Camera"
msgstr "Máy Quay Phim"

msgid "Gizmo settings for cameras."
msgstr "Gizmo settings for cameras."

# Lăng Kính -- Lens
msgid "Lens"
msgstr "Lăng Kính"

msgid "Show the gizmo to adjust the lens and orthographic size."
msgstr "Show the gizmo to adjust the lens and orthographic size."

# Khoảng Cách Hội Tụ -- Focus Distance
msgid "Focus Distance"
msgstr "Khoảng Cách Hội Tụ"

msgid "Show to gizmo to adjust the focus distance."
msgstr "Show to gizmo to adjust the focus distance."

