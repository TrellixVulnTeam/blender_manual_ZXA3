msgid "Keyframe (white / yellow diamond)"
msgstr "Khung Khóa (hình thoi màu vàng) -- Keyframe (yellow diamond)"

msgid "Normal keyframe."
msgstr "Normal keyframe."

# Phân Tích (hình thoi màu da trời) -- Breakdown (cyan small diamond) -- Breakdown (small cyan diamond)
msgid "Breakdown (small cyan diamond)"
msgstr "Phân Tích (hình thoi màu da trời) -- Breakdown (cyan small diamond)"

