# Cái/Sắp Đặt Khác -- Other
msgid "Other"
msgstr "Cái/Sắp Đặt Khác"

msgid "This page describes other curve editing tools that are not accessible via the edit menus."
msgstr "This page describes other curve editing tools that are not accessible via the edit menus."

# Vẽ Đường Cong -- Draw Curve
msgid "Draw Curve"
msgstr "Vẽ Đường Cong"

# Tham Chiếu -- Reference
msgid "Reference"
msgstr "Tham Chiếu"

msgid "Mode"
msgstr "Chế Độ"

msgid "Edit Mode"
msgstr "Chế Độ Biên Soạn"

msgid "Tool"
msgstr "Công Cụ"

msgid ":menuselection:`Toolbar --> Draw`"
msgstr ":menuselection:`Toolbar --> Draw`"

msgid "The Curve draw tool allows you to free-hand draw curves."
msgstr "The Curve draw tool allows you to free-hand draw curves."

# Lỗi/Sai Số -- Error
msgid "Error"
msgstr "Lỗi/Sai Số"

msgid "Error distance in object units. This can be seen similar to a subdivision rate for the curve. Lower values give a result that is closer to the drawing stroke while higher values give more smoothed results."
msgstr "Error distance in object units. This can be seen similar to a subdivision rate for the curve. Lower values give a result that is closer to the drawing stroke while higher values give more smoothed results."

# Phương Pháp Khớp -- Fit Method
msgid "Fit Method"
msgstr "Phương Pháp Khớp"

# Tái Khớp/Chỉnh Lại -- Refit
msgid "Refit"
msgstr "Tái Khớp/Chỉnh Lại"

msgid "Incrementally re-fits the curve (gives best results)."
msgstr "Incrementally re-fits the curve (gives best results)."

# Tách Phân -- Split
msgid "Split"
msgstr "Tách Phân"

msgid "Splits the curve until the tolerance is met (gives a better drawing performance)."
msgstr "Splits the curve until the tolerance is met (gives a better drawing performance)."

# Góc Độ Của Đỉnh -- Corner Angle
msgid "Corner Angle"
msgstr "Góc Độ Của Đỉnh"

msgid "Any angles above this are considered corners."
msgstr "Any angles above this are considered corners."

# Tuần Hoàn -- Cyclic
msgid "Cyclic"
msgstr "Tuần Hoàn"

msgid "Toggles whether or not the curve is :term:`Cyclic`."
msgstr "Toggles whether or not the curve is :term:`Tuần Hoàn -- Cyclic`."

msgid "Stroke Options"
msgstr "Stroke Options"

msgid "Panel"
msgstr "Bảng"

msgid ":menuselection:`Sidebar --> Tool --> Curve Stroke`"
msgstr ":menuselection:`Sidebar --> Tool --> Curve Stroke`"

msgid "Menu"
msgstr "Trình Đơn"

msgid ":menuselection:`Tool Settings --> Curve Stroke`"
msgstr ":menuselection:`Tool Settings --> Curve Stroke`"

msgid "Curve Stroke panel."
msgstr "Curve Stroke panel."

# Thể Loại -- Type
msgid "Type"
msgstr "Thể Loại"

msgid "Type of curve to use for drawing."
msgstr "Type of curve to use for drawing."

# Đa Giác -- Poly
msgid "Poly"
msgstr "Đa Giác"

msgid "Bézier Curve with straight line segments (auto handles)."
msgstr "Bézier Curve with straight line segments (auto handles)."

# Bézier
msgid "Bézier"
msgstr "Đường Cong Bézier"

# Sai Số -- Tolerance
msgid "Tolerance"
msgstr "Sai Số"

msgid "Lower values give a result that is closer to the drawing stroke, while higher values give more smoothed results."
msgstr "Lower values give a result that is closer to the drawing stroke, while higher values give more smoothed results."

# Phương Pháp -- Method
msgid "Method"
msgstr "Phương Pháp"

# Phát Hiện Các Góc -- Detect Corners
msgid "Detect Corners"
msgstr "Phát Hiện Các Góc"

msgid "Detects corners and uses non-aligned handles for them."
msgstr "Detects corners and uses non-aligned handles for them."

msgid "Pressure Radius"
msgstr "Pressure Radius"

# Tối Thiểu -- Min
msgid "Min"
msgstr "Tối Thiểu"

msgid "Minimum radius when the minimum pressure is applied (also the minimum when tapering)."
msgstr "Minimum radius when the minimum pressure is applied (also the minimum when tapering)."

# Tối Đa -- Max
msgid "Max"
msgstr "Tối Đa"

msgid "Radius to use when the maximum pressure is applied (or when a tablet is not used)."
msgstr "Radius to use when the maximum pressure is applied (or when a tablet is not used)."

# Chiều Sâu Phóng Chiếu -- Projection Depth
msgid "Projection Depth"
msgstr "Chiều Sâu Phóng Chiếu"

msgid "Options to control where/how the curves are drawn."
msgstr "Options to control where/how the curves are drawn."

# Con Trỏ -- Cursor
msgid "Cursor"
msgstr "Con Trỏ"

msgid "Uses the depth under the cursor to draw curves."
msgstr "Uses the depth under the cursor to draw curves."

# Bề Mặt -- Surface
msgid "Surface"
msgstr "Bề Mặt"

msgid "Used to draw on top of other objects."
msgstr "Used to draw on top of other objects."

# Dịch Chuyển -- Offset
msgid "Offset"
msgstr "Dịch Chuyển"

msgid "Distance to offset the curve from the surface."
msgstr "Distance to offset the curve from the surface."

# Dịch Chuyển Tuyệt Đối -- Absolute Offset
msgid "Absolute Offset"
msgstr "Dịch Chuyển Tuyệt Đối"

msgid "Applies a fixed offset (does not scale by the curve radius)."
msgstr "Applies a fixed offset (does not scale by the curve radius)."

# Duy Điểm Đầu -- Only First
msgid "Only First"
msgstr "Duy Điểm Đầu"

msgid "Only uses the start of the stroke for the depth."
msgstr "Only uses the start of the stroke for the depth."

# Normal/View
msgid "Normal/View"
msgstr "Pháp Tuyến/Góc Nhìn"

msgid "Draws perpendicular to the surface."
msgstr "Draws perpendicular to the surface."

# Normal/Surface
msgid "Normal/Surface"
msgstr "Pháp Tuyến/Bề Mặt"

msgid "Draws aligned to the surface."
msgstr "Draws aligned to the surface."

# Góc Nhìn -- View
msgid "View"
msgstr "Góc Nhìn"

msgid "Draws aligned to the viewport."
msgstr "Draws aligned to the viewport."

# Đặt Trọng Lượng Đích -- Set Goal Weight
msgid "Set Goal Weight"
msgstr "Đặt Trọng Lượng Đích"

msgid ":menuselection:`Context Menu --> Set Goal Weight`"
msgstr ":menuselection:`Context Menu --> Set Goal Weight`"

msgid "Sets the curve's :ref:`Weight <curves-weight>` for the selected control point to the specified value. If more than one control point is selected this will set the *Mean Weight*."
msgstr "Sets the curve's :ref:`Trọng Lượng -- Weight <curves-weight>` for the selected control point to the specified value. If more than one control point is selected this will set the *Trọng Lượng Trung Bình -- Mean Weight*."

# Thêm Điểm Đỉnh -- Add Vertex
msgid "Add Vertex"
msgstr "Thêm Điểm Đỉnh"

msgid "Hotkey"
msgstr "Phím Tắt"

# :kbd:`:kbd:`Ctrl-NCP (RMB)``
msgid ":kbd:`Ctrl-RMB`"
msgstr ":kbd:`Ctrl-LMB`"

msgid "Interactively places new points with :kbd:`Ctrl-RMB` at the cursor position. With the selection it deals in same manner as the *Extrude Curve and Move* tool."
msgstr "Interactively places new points with :kbd:`:kbd:`Ctrl-NCP (RMB)`` at the cursor position. With the selection it deals in same manner as the *Đẩy Trồi/Nới Đường Cong và Di Chuyển -- Extrude Curve and Move* tool."

