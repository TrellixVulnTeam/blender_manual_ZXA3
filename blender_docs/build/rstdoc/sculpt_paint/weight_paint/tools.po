msgid "Weight Paint Tools"
msgstr "Weight Paint Tools"

# Vẽ -- Draw
msgid "Draw"
msgstr "Vẽ"

msgid "Paints a specified weight over the object."
msgstr "Paints a specified weight over the object."

# Làm Mờ -- Blur
msgid "Blur"
msgstr "Làm Mờ"

msgid "Smooths out the weighting of adjacent vertices. In this mode the Weight Value is ignored. The strength defines how much the smoothing is applied."
msgstr "Smooths out the weighting of adjacent vertices. In this mode the Weight Value is ignored. The strength defines how much the smoothing is applied."

# Trung Bình Hóa -- Average
msgid "Average"
msgstr "Trung Bình Hóa"

msgid "Smooths weights by painting the average resulting weight from all weights under the brush."
msgstr "Smooths weights by painting the average resulting weight from all weights under the brush."

# Bôi Nhòe -- Smear
msgid "Smear"
msgstr "Bôi Nhòe"

msgid "Smudges weights by grabbing the weights under the brush and \"dragging\" them. This can be imagined as a finger painting tool."
msgstr "Smudges weights by grabbing the weights under the brush and \"dragging\" them. This can be imagined as a finger painting tool."

# Dốc Màu -- Gradient
msgid "Gradient"
msgstr "Dốc Màu"

msgid "Applies a linear/radial weight gradient; this is useful at times when painting gradual changes in weight becomes difficult. Blends the weights of selected vertices with unselected vertices."
msgstr "Applies a linear/radial weight gradient; this is useful at times when painting gradual changes in weight becomes difficult. Blends the weights of selected vertices with unselected vertices."

msgid "Example of the Gradient tool being used with selected vertices."
msgstr "Example of the Gradient tool being used with selected vertices."

# Trọng Lượng -- Weight
msgid "Weight"
msgstr "Trọng Lượng"

msgid "The gradient starts at the current selected weight value, blending out to nothing."
msgstr "The gradient starts at the current selected weight value, blending out to nothing."

# Độ Đậm/Sức Mạnh/Cường Độ -- Strength
msgid "Strength"
msgstr "Độ Đậm/Sức Mạnh/Cường Độ"

msgid "Lower values can be used so the gradient mixes in with the existing weights (just like with the brush)."
msgstr "Lower values can be used so the gradient mixes in with the existing weights (just like with the brush)."

# Thể Loại -- Type
msgid "Type"
msgstr "Thể Loại"

msgid "Linear: :kbd:`Alt-LMB` and drag."
msgstr "Linear: :kbd:`:kbd:`Alt-NCT (LMB)`` and drag."

msgid "Radial: :kbd:`Ctrl-Alt-LMB` and drag."
msgstr "Radial: :kbd:`:kbd:`Ctrl-Alt-NCT (LMB)`` and drag."

# Lấy Mẫu Vật -- Sample
msgid "Sample"
msgstr "Lấy Mẫu Vật"

# Trọng Lượng -- Weights
msgid "Weights"
msgstr "Trọng Lượng"

msgid "Sets the brush *Weight* as the weight selected under the cursor."
msgstr "Sets the brush *Trọng Lượng -- Weight* as the weight selected under the cursor."

# Nhóm Điểm Đỉnh -- Vertex Group
msgid "Vertex Group"
msgstr "Nhóm Điểm Đỉnh"

msgid "Displays a list of possible vertex groups to select that are under the cursor."
msgstr "Displays a list of possible vertex groups to select that are under the cursor."

msgid ":ref:`Annotate <tool-annotate>`"
msgstr ":ref:`Chú Thích -- Annotate <tool-annotate>`"

msgid "Draw free-hand annotation."
msgstr "Draw free-hand annotation."

# Chú Thích Bằng Đường Thẳng -- Annotate Line
msgid "Annotate Line"
msgstr "Chú Thích Bằng Đường Thẳng"

msgid "Draw straight line annotation."
msgstr "Draw straight line annotation."

# Chú Thích Bằng Đa Giác -- Annotate Polygon
msgid "Annotate Polygon"
msgstr "Chú Thích Bằng Đa Giác"

msgid "Draw a polygon annotation."
msgstr "Draw a polygon annotation."

# Tẩy Xóa Chú Thích -- Annotate Eraser
msgid "Annotate Eraser"
msgstr "Tẩy Xóa Chú Thích"

msgid "Erase previous drawn annotations."
msgstr "Erase previous drawn annotations."

