msgid "Swirl Visual Effect"
msgstr "Swirl Visual Effect"

msgid "The *Swirl* Visual Effect applies a swirling pattern to the object. The effect use an object as the center of the swirl."
msgstr "The *Xoáy Lốc -- Swirl* Visual Effect applies a swirling pattern to the object. The effect use an object as the center of the swirl."

# Tùy Chọn -- Options
msgid "Options"
msgstr "Tùy Chọn"

msgid "Shadow Visual Effect."
msgstr "Shadow Visual Effect."

# Vật Thể -- Object
msgid "Object"
msgstr "Vật Thể"

msgid "Sets the object to use as the center of the swirl."
msgstr "Sets the object to use as the center of the swirl."

# Bán Kính -- Radius
msgid "Radius"
msgstr "Bán Kính"

msgid "External radius size of the swirl."
msgstr "External radius size of the swirl."

# Góc Độ -- Angle
msgid "Angle"
msgstr "Góc Độ"

msgid "Rotation angle of the swirl. A value of 0 shows no swirl."
msgstr "Rotation angle of the swirl. A value of 0 shows no swirl."

# Trong Suốt -- Transparent
msgid "Transparent"
msgstr "Trong Suốt"

msgid "When enabled, the Object is fully transparent outside the radius of the swirl."
msgstr "When enabled, the Object is fully transparent outside the radius of the swirl."

# Ví Dụ -- Example
msgid "Example"
msgstr "Ví Dụ"

msgid "Swirl Effect samples (with a Radius of 100 px)."
msgstr "Swirl Effect samples (with a Radius of 100 px)."

msgid "Angle: 0°."
msgstr "Angle: 0°."

msgid "Angle: 15°."
msgstr "Angle: 15°."

msgid "Angle: 45°."
msgstr "Angle: 45°."

msgid "Angle: 90°."
msgstr "Angle: 90°."

