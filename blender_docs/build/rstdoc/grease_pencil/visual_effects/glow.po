msgid "Glow Visual Effect"
msgstr "Glow Visual Effect"

msgid "The *Glow* Visual Effect add a glowing rim around the object."
msgstr "The *Hừng Sáng -- Glow* Visual Effect add a glowing rim around the object."

# Tùy Chọn -- Options
msgid "Options"
msgstr "Tùy Chọn"

msgid "Glow Visual Effect."
msgstr "Glow Visual Effect."

# Chế Độ -- Mode
msgid "Mode"
msgstr "Chế Độ"

msgid "Determines the mode for the glow effect."
msgstr "Determines the mode for the glow effect."

# Ngời Sáng -- Luminance
msgid "Luminance"
msgstr "Ngời Sáng"

msgid "The glow light illuminates the entire object."
msgstr "The glow light illuminates the entire object."

# Màu Sắc -- Color
msgid "Color"
msgstr "Màu Sắc"

msgid "The glow light only affect a single color."
msgstr "The glow light only affect a single color."

# Màu Hừng Sáng -- Glow Color
msgid "Glow Color"
msgstr "Màu Hừng Sáng"

msgid "Defines the glow color."
msgstr "Defines the glow color."

msgid "When Color Mode is selected, allows to select a single color to apply the glow light."
msgstr "When Color Mode is selected, allows to select a single color to apply the glow light."

# Ngưỡng -- Threshold
msgid "Threshold"
msgstr "Ngưỡng"

msgid "When Luminance Mode is selected, limits the colors affected by the glow light. (A value of 1 means no colors affected.)"
msgstr "When Luminance Mode is selected, limits the colors affected by the glow light. (A value of 1 means no colors affected.)"

# Bán Kính -- Radius
msgid "Radius"
msgstr "Bán Kính"

msgid "Adds a blur effect to the glow. If 0 no blur is used."
msgstr "Adds a blur effect to the glow. If 0 no blur is used."

# Lượng Mẫu Vật -- Samples
msgid "Samples"
msgstr "Lượng Mẫu Vật"

msgid "Number of Blur samples (0 disabled the blur effect)."
msgstr "Number of Blur samples (0 disabled the blur effect)."

# Sử Dụng Chế Độ Alpha -- Use Alpha Mode
msgid "Use Alpha Mode"
msgstr "Sử Dụng Chế Độ Alpha"

msgid "When enabled, glow only affects alpha areas."
msgstr "When enabled, glow only affects alpha areas."

# Ví Dụ -- Example
msgid "Example"
msgstr "Ví Dụ"

msgid "Glow Effect samples."
msgstr "Glow Effect samples."

msgid "Original image."
msgstr "Hình ảnh gốc."

msgid "Mode: Luminance."
msgstr "Mode: Luminance."

msgid "Mode: Luminance (Alpha mode)."
msgstr "Mode: Luminance (Alpha mode)."

msgid "Mode: Color (Black lines)."
msgstr "Mode: Color (Black lines)."

