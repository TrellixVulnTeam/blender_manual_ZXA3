msgid "Shadow Visual Effect"
msgstr "Shadow Visual Effect"

msgid "The *Shadow* Visual Effect shows a simulated shadow casting by the object."
msgstr "The *Ngả/Bóng Tối -- Shadow* Visual Effect shows a simulated shadow casting by the object."

msgid "For simulating the shadow a color silhouette of the object is displaced in horizontal and/or vertical direction on the back of the object."
msgstr "For simulating the shadow a color silhouette of the object is displaced in horizontal and/or vertical direction on the back of the object."

# Tùy Chọn -- Options
msgid "Options"
msgstr "Tùy Chọn"

msgid "Shadow Visual Effect."
msgstr "Shadow Visual Effect."

# Dịch Chuyển -- Offset
msgid "Offset"
msgstr "Dịch Chuyển"

msgid "Control the shadow displacement in pixels on the X and Z axis."
msgstr "Control the shadow displacement in pixels on the X and Z axis."

msgid "X, Z"
msgstr "X, Z"

# Màu Bóng Tối -- Shadow Color
msgid "Shadow Color"
msgstr "Màu Bóng Tối"

msgid "Defines the shadow color."
msgstr "Defines the shadow color."

# Tỷ Lệ -- Scale
msgid "Scale"
msgstr "Tỷ Lệ"

msgid "Control the size of the shadow on the X and Z axis."
msgstr "Control the size of the shadow on the X and Z axis."

# Xoay Chiều -- Rotation
msgid "Rotation"
msgstr "Xoay Chiều"

msgid "Sets the shadow rotation around the *Grease Pencil* object center or another object when *Use Object As Pivot* is enabled."
msgstr "Sets the shadow rotation around the *Bút Chì Dầu -- Grease Pencil* object center or another object when *Dùng Vật Thể Làm Điểm Tựa -- Use Object As Pivot* is enabled."

# Làm Mờ -- Blur
msgid "Blur"
msgstr "Làm Mờ"

msgid "Control the blur scale in pixels on the X and Z axis."
msgstr "Control the blur scale in pixels on the X and Z axis."

# Lượng Mẫu Vật -- Samples
msgid "Samples"
msgstr "Lượng Mẫu Vật"

msgid "Number of blur samples (0 disabled the blur effect)."
msgstr "Number of blur samples (0 disabled the blur effect)."

# Dùng Vật Thể Làm Điểm Tựa -- Use Object As Pivot
msgid "Use Object As Pivot"
msgstr "Dùng Vật Thể Làm Điểm Tựa"

msgid "When enabled, an Object is used by the shadow as the center of rotation."
msgstr "When enabled, an Object is used by the shadow as the center of rotation."

# Vật Thể -- Object
msgid "Object"
msgstr "Vật Thể"

msgid "Sets the object to use as the pivot point for the shadow."
msgstr "Sets the object to use as the pivot point for the shadow."

# Sử Dụng Hiệu Ứng Sóng -- Use Wave Effect
msgid "Use Wave Effect"
msgstr "Sử Dụng Hiệu Ứng Sóng"

msgid "When enabled, apply a wave distortion to the shadow."
msgstr "When enabled, apply a wave distortion to the shadow."

# Chiều Hướng -- Direction
msgid "Direction"
msgstr "Chiều Hướng"

msgid "Sets horizontal or vertical direction for the waves."
msgstr "Sets horizontal or vertical direction for the waves."

# Biên Độ -- Amplitude
msgid "Amplitude"
msgstr "Biên Độ"

msgid "Controls the strength and the depth of the wave."
msgstr "Controls the strength and the depth of the wave."

# Chu Kỳ -- Period
msgid "Period"
msgstr "Chu Kỳ"

msgid "Controls the wave period. The time it takes to complete one cycle."
msgstr "Controls the wave period. The time it takes to complete one cycle."

# Pha Sóng -- Phase
msgid "Phase"
msgstr "Pha Sóng"

msgid "Shifts the wave pattern over the shadow."
msgstr "Shifts the wave pattern over the shadow."

# Ví Dụ -- Example
msgid "Example"
msgstr "Ví Dụ"

msgid "Shadow Effect samples."
msgstr "Shadow Effect samples."

msgid "Simple Shadow."
msgstr "Simple Shadow."

msgid "Blurred Shadow."
msgstr "Blurred Shadow."

msgid "Stretched shadow with an Empty as center of rotation."
msgstr "Stretched shadow with an Empty as center of rotation."

