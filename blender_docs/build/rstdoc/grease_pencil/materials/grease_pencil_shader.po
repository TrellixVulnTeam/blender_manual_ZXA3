# Hệ Số Pha Trộn -- Mix Factor
msgid "Mix Factor"
msgstr "Hệ Số Pha Trộn"

msgid "Primary and secondary colors mixing amount."
msgstr "Primary and secondary colors mixing amount."

# Đảo Màu -- Flip colors
msgid "Flip colors"
msgstr "Đảo Màu"

msgid "Flips the gradient, inverting the primary and secondary colors."
msgstr "Flips the gradient, inverting the primary and secondary colors."

# Vị Trí -- Location
msgid "Location"
msgstr "Vị Trí"

msgid "Shifts the gradient position."
msgstr "Shifts the gradient position."

msgid "X, Y"
msgstr "X, Y"

# Tỷ Lệ -- Scale
msgid "Scale"
msgstr "Tỷ Lệ"

msgid "Scales the gradient."
msgstr "Scales the gradient."

# Góc Độ -- Angle
msgid "Angle"
msgstr "Góc Độ"

msgid "Rotates the gradient."
msgstr "Rotates the gradient."

msgid "Checkerboard"
msgstr "Checkerboard"

msgid "Use a checkered pattern."
msgstr "Use a checkered pattern."

