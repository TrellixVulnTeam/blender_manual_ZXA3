# Hướng Dẫn -- Guides
msgid "Guides"
msgstr "Hướng Dẫn"

# Tham Chiếu -- Reference
msgid "Reference"
msgstr "Tham Chiếu"

msgid "Mode"
msgstr "Chế Độ"

msgid "Draw Mode"
msgstr "Chế Độ Vẽ"

msgid "Header"
msgstr "Tiêu Đề"

msgid ":menuselection:`Guides`"
msgstr ":menuselection:`:menuselection:`Hướng Dẫn (Guides)``"

msgid "Guides are drawing aids that make it easier to create different types of strokes. The Guides can be activated with the button next to the selector (grid icon)."
msgstr "Guides are drawing aids that make it easier to create different types of strokes. The Guides can be activated with the button next to the selector (grid icon)."

msgid "Guide Types"
msgstr "Guide Types"

msgid "Guide selector activated in the 3D View header."
msgstr "Guide selector activated in the 3D View header."

# Vòng Tròn -- Circular
msgid "Circular"
msgstr "Vòng Tròn"

msgid "Constrains the drawing of new strokes to form rings from the selected reference point."
msgstr "Constrains the drawing of new strokes to form rings from the selected reference point."

# Tỏa Tròn -- Radial
msgid "Radial"
msgstr "Tỏa Tròn"

msgid "Constrains the drawing of new strokes to form rays from the selected reference point."
msgstr "Constrains the drawing of new strokes to form rays from the selected reference point."

# Song Song -- Parallel
msgid "Parallel"
msgstr "Song Song"

msgid "Constrains the drawing of new strokes to form parallel lines."
msgstr "Constrains the drawing of new strokes to form parallel lines."

# Góc Độ -- Angle
msgid "Angle"
msgstr "Góc Độ"

msgid "Angle direction of the parallel lines."
msgstr "Angle direction of the parallel lines."

# Khung Lưới/Đồ Thị -- Grid
msgid "Grid"
msgstr "Khung Lưới/Đồ Thị"

msgid "Constrains the drawing of new strokes to form parallel horizontal or vertical lines."
msgstr "Constrains the drawing of new strokes to form parallel horizontal or vertical lines."

# Đẳng Cự -- Isometric
msgid "Isometric"
msgstr "Đẳng Cự"

msgid "Constrains the drawing of new strokes to vertical or isometric lines."
msgstr "Constrains the drawing of new strokes to vertical or isometric lines."

msgid "Angle direction of the isometric lines."
msgstr "Angle direction of the isometric lines."

# Tùy Chọn Chung -- Common Options
msgid "Common Options"
msgstr "Tùy Chọn Chung"

# Sử Dụng Bám Dính -- Use Snapping
msgid "Use Snapping"
msgstr "Sử Dụng Bám Dính"

msgid "When enabled, snap the drawn strokes to an angle or spacing."
msgstr "When enabled, snap the drawn strokes to an angle or spacing."

# Phân Khoảng -- Spacing
msgid "Spacing"
msgstr "Phân Khoảng"

msgid "Guide spacing."
msgstr "Guide spacing."

# Điểm Tham Chiếu -- Reference Point
msgid "Reference Point"
msgstr "Điểm Tham Chiếu"

msgid "Determines the origin point to use for the creation of the lines. Applies only for *Circular* and *Radial* guides."
msgstr "Determines the origin point to use for the creation of the lines. Applies only for *Vòng Tròn -- Circular* and *Tỏa Tròn -- Radial* guides."

# Con Trỏ -- Cursor
msgid "Cursor"
msgstr "Con Trỏ"

msgid "Use the cursor as a reference point."
msgstr "Use the cursor as a reference point."

# Tùy Chỉnh -- Custom
msgid "Custom"
msgstr "Tùy Chỉnh"

msgid "Use a custom location as a reference point."
msgstr "Use a custom location as a reference point."

# Vị Trí Tùy Chọn -- Custom Location
msgid "Custom Location"
msgstr "Vị Trí Tùy Chọn"

msgid "X, Y Z"
msgstr "X, Y Z"

# Vật Thể -- Object
msgid "Object"
msgstr "Vật Thể"

msgid "Use an object as a reference point."
msgstr "Use an object as a reference point."

msgid "An :ref:`Data ID menu <ui-data-id>` to select the object (usually an empty), which location will be used as a reference point."
msgstr "An :ref:`Data ID menu <ui-data-id>` to select the object (usually an empty), which location will be used as a reference point."

# Ví Dụ -- Examples
msgid "Examples"
msgstr "Ví Dụ"

msgid "Examples of strokes using different types of Guides."
msgstr "Examples of strokes using different types of Guides."

msgid "Circular Guides."
msgstr "Circular Guides."

msgid "Radial Guides."
msgstr "Radial Guides."

msgid "Parallel Guides (30° Angle)."
msgstr "Parallel Guides (30° Angle)."

msgid "Grid Guides."
msgstr "Grid Guides."

