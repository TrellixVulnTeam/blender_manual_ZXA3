msgid "Stroke Menu"
msgstr "Stroke Menu"

msgid "This page covers many of the tools in the *Strokes* menu. These are tools that work primarily on strokes, however, some also work with point selections."
msgstr "This page covers many of the tools in the *Nét Vẽ -- Strokes* menu. These are tools that work primarily on strokes, however, some also work with point selections."

# Phân Chia -- Subdivide
msgid "Subdivide"
msgstr "Phân Chia"

# Tham Chiếu -- Reference
msgid "Reference"
msgstr "Tham Chiếu"

msgid "Mode"
msgstr "Chế Độ"

msgid "Edit Mode"
msgstr "Chế Độ Biên Soạn"

msgid "Menu"
msgstr "Trình Đơn"

msgid ":menuselection:`Stroke --> Subdivide`"
msgstr ":menuselection:`Stroke --> Subdivide`"

msgid "Subdivides the strokes by inserting points between the selected points."
msgstr "Subdivides the strokes by inserting points between the selected points."

# Số Phân Đoạn -- Number of Cuts
msgid "Number of Cuts"
msgstr "Số Phân Đoạn"

msgid "The number of subdivisions to perform."
msgstr "The number of subdivisions to perform."

# Làm Mịn/Trơn Tru -- Smooth
msgid "Smooth"
msgstr "Làm Mịn/Trơn Tru"

msgid "The amount of the smoothness on subdivided points."
msgstr "The amount of the smoothness on subdivided points."

# Lặp Lại -- Repeat
msgid "Repeat"
msgstr "Lặp Lại"

msgid "Number of times to repeat the procedure."
msgstr "Number of times to repeat the procedure."

# Điểm được Chọn -- Selected points
msgid "Selected points"
msgstr "Điểm được Chọn"

msgid "When enabled, limits the effect to only the selected points within the stroke."
msgstr "When enabled, limits the effect to only the selected points within the stroke."

# Vị Trí -- Position
msgid "Position"
msgstr "Vị Trí"

msgid "When enabled, the operator affect the points location."
msgstr "When enabled, the operator affect the points location."

# Độ Dày -- Thickness
msgid "Thickness"
msgstr "Độ Dày"

msgid "When enabled, the operator affect the points thickness."
msgstr "When enabled, the operator affect the points thickness."

# Độ Đậm/Sức Mạnh/Cường Độ -- Strength
msgid "Strength"
msgstr "Độ Đậm/Sức Mạnh/Cường Độ"

msgid "When enabled, the operator affect the points strength (alpha)."
msgstr "When enabled, the operator affect the points strength (alpha)."

# UVs
msgid "UVs"
msgstr "UV"

msgid "When enabled, the operator affect the UV rotation on the points."
msgstr "When enabled, the operator affect the UV rotation on the points."

# Đơn Giản Hóa -- Simplify
msgid "Simplify"
msgstr "Đơn Giản Hóa"

msgid ":menuselection:`Stroke --> Simplify`"
msgstr ":menuselection:`Stroke --> Simplify`"

msgid "Reduce the amount of points in the strokes."
msgstr "Reduce the amount of points in the strokes."

# Cố Định/Bất Biến/Định Trước -- Fixed
msgid "Fixed"
msgstr "Cố Định/Bất Biến/Định Trước"

msgid "Deletes alternated points in the strokes, except the start and end points."
msgstr "Deletes alternated points in the strokes, except the start and end points."

# Số Bước -- Steps
msgid "Steps"
msgstr "Số Bước"

msgid "The number of times to repeat the procedure."
msgstr "The number of times to repeat the procedure."

# Tùy Ứng/Ứng Đối/Thích Nghi -- Adaptive
msgid "Adaptive"
msgstr "Tùy Ứng/Ứng Đối/Thích Nghi"

msgid "Uses the RDP algorithm (Ramer-Douglas-Peucker algorithm) for points deletion. The algorithm tries to obtain a similar line shape with fewer points."
msgstr "Uses the RDP algorithm (Ramer-Douglas-Peucker algorithm) for points deletion. The algorithm tries to obtain a similar line shape with fewer points."

# Hệ Số -- Factor
msgid "Factor"
msgstr "Hệ Số"

msgid "Controls the amount of recursively simplifications applied by the algorithm."
msgstr "Controls the amount of recursively simplifications applied by the algorithm."

# Lấy Mẫu Vật -- Sample
msgid "Sample"
msgstr "Lấy Mẫu Vật"

msgid "Recreates the stroke geometry with a predefined length between points."
msgstr "Recreates the stroke geometry with a predefined length between points."

# Chiều Dài -- Length
msgid "Length"
msgstr "Chiều Dài"

msgid "The distance between points on the recreated stroke. Smaller values will require more points to recreate the stroke, while larger values will result in fewer points needed to recreate the curve."
msgstr "The distance between points on the recreated stroke. Smaller values will require more points to recreate the stroke, while larger values will result in fewer points needed to recreate the curve."

# Xén -- Trim
msgid "Trim"
msgstr "Xén"

msgid ":menuselection:`Stroke --> Trim`"
msgstr ":menuselection:`Stroke --> Trim`"

msgid "Trims selected stroke to first loop or intersection."
msgstr "Trims selected stroke to first loop or intersection."

msgid "Original stroke."
msgstr "Original stroke."

msgid "Result of trim operation."
msgstr "Result of trim operation."

# Chắp Nối -- Join
msgid "Join"
msgstr "Chắp Nối"

msgid ":menuselection:`Stroke --> Join --> Join, Join and Copy`"
msgstr ":menuselection:`Stroke --> Join --> Join, Join and Copy`"

msgid "Hotkey"
msgstr "Phím Tắt"

msgid ":kbd:`Ctrl-J`, :kbd:`Shift-Ctrl-J`"
msgstr ":kbd:`:kbd:`Ctrl-J``, :kbd:`:kbd:`Shift-Ctrl-J``"

msgid "Join two or more strokes into a single one."
msgstr "Join two or more strokes into a single one."

# Thể Loại -- Type
msgid "Type"
msgstr "Thể Loại"

msgid "Join :kbd:`Ctrl-J`"
msgstr "Join :kbd:`Ctrl-J` -- Join :kbd:`Ctrl-J`trl-J``"

msgid "Join selected strokes by connecting points."
msgstr "Join selected strokes by connecting points."

msgid "Join and Copy :kbd:`Shift-Ctrl-J`"
msgstr "Join and Copy :kbd:`Shift-Ctrl-J` -- Join and Copy :kbd:`Shift-Ctrl-J`trl-J``"

msgid "Join selected strokes by connecting points in a new stroke."
msgstr "Join selected strokes by connecting points in a new stroke."

# Để Nguyên Các Khoảng Trống -- Leave Gaps
msgid "Leave Gaps"
msgstr "Để Nguyên Các Khoảng Trống"

msgid "When enabled, do not use geometry to connect the strokes."
msgstr "When enabled, do not use geometry to connect the strokes."

# Di Chuyển đến Tầng Lớp -- Move to Layer
msgid "Move to Layer"
msgstr "Di Chuyển đến Tầng Lớp"

msgid ":menuselection:`Stroke --> Move to Layer`"
msgstr ":menuselection:`Stroke --> Move to Layer`"

# :kbd:`:kbd:`M``
msgid ":kbd:`M`"
msgstr ":kbd:`NumpadSlash`"

msgid "A pop-up menu to move the stroke to a different layer. You can choose the layer to move the selected strokes to from a list of layers of the current *Grease Pencil* object. You can also add a new layer to move the selected stroke to."
msgstr "A pop-up menu to move the stroke to a different layer. You can choose the layer to move the selected strokes to from a list of layers of the current *Bút Chì Dầu -- Grease Pencil* object. You can also add a new layer to move the selected stroke to."

# Ấn Định Nguyên Vật Liệu -- Assign Material
msgid "Assign Material"
msgstr "Ấn Định Nguyên Vật Liệu"

msgid ":menuselection:`Stroke --> Assign Material`"
msgstr ":menuselection:`Stroke --> Assign Material`"

msgid "Changes the material linked to the selected stroke. You can choose the name of the material to be used by the selected stroke from a list of materials of the current *Grease Pencil* object."
msgstr "Changes the material linked to the selected stroke. You can choose the name of the material to be used by the selected stroke from a list of materials of the current *Bút Chì Dầu -- Grease Pencil* object."

# Đặt làm Nguyên Liệu Đang Hoạt Động -- Set as Active Material
msgid "Set as Active Material"
msgstr "Đặt làm Nguyên Liệu Đang Hoạt Động"

msgid ":menuselection:`Stroke --> Set as Active Material`"
msgstr ":menuselection:`Stroke --> Set as Active Material`"

msgid "Sets the active object material based on the selected stroke material."
msgstr "Sets the active object material based on the selected stroke material."

# Bố Trí Nét Vẽ -- Arrange Strokes
msgid "Arrange Strokes"
msgstr "Bố Trí Nét Vẽ"

msgid ":menuselection:`Stroke --> Arrange Strokes`"
msgstr ":menuselection:`Stroke --> Arrange Strokes`"

msgid "Change the drawing order of the strokes in the 2D layer."
msgstr "Change the drawing order of the strokes in the 2D layer."

# Đưa về Trước Nhất -- Bring to Front
msgid "Bring to Front"
msgstr "Đưa về Trước Nhất"

msgid "Moves to the top the selected points/strokes."
msgstr "Moves to the top the selected points/strokes."

# Đưa Về Trước -- Bring Forward
msgid "Bring Forward"
msgstr "Đưa Về Trước"

msgid "Moves the selected points/strokes upper the next one in the drawing order."
msgstr "Moves the selected points/strokes upper the next one in the drawing order."

# Đưa Về Sau -- Send Backward
msgid "Send Backward"
msgstr "Đưa Về Sau"

msgid "Moves the selected points/strokes below the previous one in the drawing order."
msgstr "Moves the selected points/strokes below the previous one in the drawing order."

# Đưa về Sau Cùng -- Send to Back
msgid "Send to Back"
msgstr "Đưa về Sau Cùng"

msgid "Moves to the bottom the selected points/strokes."
msgstr "Moves to the bottom the selected points/strokes."

# Đóng/Kín -- Close
msgid "Close"
msgstr "Đóng/Kín"

msgid ":menuselection:`Stroke --> Close`"
msgstr ":menuselection:`Stroke --> Close`"

msgid ":kbd:`F`"
msgstr ":kbd:`:kbd:`F``"

msgid "Close or open strokes by connecting the last and first point."
msgstr "Close or open strokes by connecting the last and first point."

# Đóng Toàn Bộ -- Close all
msgid "Close all"
msgstr "Đóng Toàn Bộ"

msgid "Close all open selected strokes."
msgstr "Close all open selected strokes."

# Mở Toàn Bộ -- Open all
msgid "Open all"
msgstr "Mở Toàn Bộ"

msgid "Open all closed selected strokes."
msgstr "Open all closed selected strokes."

# Đảo Lật/Bật/Tắt -- Toggle
msgid "Toggle"
msgstr "Đảo Lật/Bật/Tắt"

msgid "Close or Open selected strokes as required."
msgstr "Close or Open selected strokes as required."

# Kiến Tạo Hình Học -- Create geometry
msgid "Create geometry"
msgstr "Kiến Tạo Hình Học"

msgid "When enabled, points are added for closing the strokes. If disabled, the operator act the same as *Toggle Cyclic*."
msgstr "When enabled, points are added for closing the strokes. If disabled, the operator act the same as *Bật/Tắt Tính Tuần Hoàn -- Toggle Cyclic*."

# Bật/Tắt Tính Tuần Hoàn -- Toggle Cyclic
msgid "Toggle Cyclic"
msgstr "Bật/Tắt Tính Tuần Hoàn"

msgid ":menuselection:`Stroke --> Toggle Cyclic`"
msgstr ":menuselection:`Stroke --> Toggle Cyclic`"

msgid "Toggles between an open stroke and closed stroke (cyclic)."
msgstr "Toggles between an open stroke and closed stroke (cyclic)."

msgid "When enabled, points are added for closing the strokes like when using the *Close* tool. If disabled, the stroke is close without any actual geometry."
msgstr "When enabled, points are added for closing the strokes like when using the *Đóng/Kín -- Close* tool. If disabled, the stroke is close without any actual geometry."

# Đảo Hình Đầu Nét -- Toggle Caps
msgid "Toggle Caps"
msgstr "Đảo Hình Đầu Nét"

msgid ":menuselection:`Stroke --> Toggle Caps`"
msgstr ":menuselection:`Stroke --> Toggle Caps`"

msgid "Toggle ending cap styles of the stroke."
msgstr "Toggle ending cap styles of the stroke."

# Default
msgid "Default"
msgstr "Mặc Định"

msgid "Sets stroke start and end points to rounded (default)."
msgstr "Sets stroke start and end points to rounded (default)."

# Cả Hai -- Both
msgid "Both"
msgstr "Cả Hai"

msgid "Toggle stroke start and end points caps to flat or rounded."
msgstr "Toggle stroke start and end points caps to flat or rounded."

# Khởi Đầu -- Start
msgid "Start"
msgstr "Khởi Đầu"

msgid "Toggle stroke start point cap to flat or rounded."
msgstr "Toggle stroke start point cap to flat or rounded."

# End
msgid "End"
msgstr "Kết Thúc"

msgid "Toggle stroke end point cap to flat or rounded."
msgstr "Toggle stroke end point cap to flat or rounded."

msgid "Stroke ending with rounded caps."
msgstr "Stroke ending with rounded caps."

msgid "Stroke ending with flat caps."
msgstr "Stroke ending with flat caps."

msgid "Stroke ending with combined caps."
msgstr "Stroke ending with combined caps."

# Đảo Chiều Hướng -- Switch Direction
msgid "Switch Direction"
msgstr "Đảo Chiều Hướng"

msgid ":menuselection:`Stroke --> Switch Direction`"
msgstr ":menuselection:`Stroke --> Switch Direction`"

msgid "Reverse the direction of the points in the selected strokes (i.e. the start point will become the end one, and vice versa)."
msgstr "Reverse the direction of the points in the selected strokes (i.e. the start point will become the end one, and vice versa)."

