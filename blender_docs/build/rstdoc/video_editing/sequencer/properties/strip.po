# Dải -- Strip
msgid "Strip"
msgstr "Dải"

# Tiêu Đề -- Header
msgid "Header"
msgstr "Tiêu Đề"

# Thể Loại -- Type
msgid "Type"
msgstr "Thể Loại"

msgid "Strip type represented by an icon."
msgstr "Strip type represented by an icon."

# Tên -- Name
msgid "Name"
msgstr "Tên"

msgid "You can name or rename your strips here."
msgstr "You can name or rename your strips here."

msgid "Mute (checkbox)"
msgstr "Mute (checkbox)"

msgid "Strip will not produce any output."
msgstr "Strip will not produce any output."

msgid "Adjust Panel"
msgstr "Adjust Panel"

# Tham Chiếu -- Reference
msgid "Reference"
msgstr "Tham Chiếu"

msgid "Panel"
msgstr "Bảng"

msgid ":menuselection:`Sidebar region --> Strip --> Adjust`"
msgstr ":menuselection:`Sidebar region --> Strip --> Adjust`"

msgid "The *Adjust* panel is used to control visual properties of strips."
msgstr "The *Điều Chỉnh -- Adjust* panel is used to control visual properties of strips."

# Tổng Hợp -- Compositing
msgid "Compositing"
msgstr "Tổng Hợp"

# Hòa Trộn/Chuyển Đổi -- Blend
msgid "Blend"
msgstr "Hòa Trộn/Chuyển Đổi"

msgid "Mode of blending strip with lower channels."
msgstr "Mode of blending strip with lower channels."

# Độ Đục -- Opacity
msgid "Opacity"
msgstr "Độ Đục"

msgid "Set the opacity (alpha) of the strip."
msgstr "Set the opacity (alpha) of the strip."

# Biến Hóa -- Transform
msgid "Transform"
msgstr "Biến Hóa"

# Đối Xứng Hóa X -- Mirror X
msgid "Mirror X"
msgstr "Đối Xứng Hóa X"

msgid "Mirrors the image along the X axis (left to right)."
msgstr "Mirrors the image along the X axis (left to right)."

# Đối Xứng Y -- Mirror Y
msgid "Mirror Y"
msgstr "Đối Xứng Y"

msgid "Mirrors the image along the Y axis (top to bottom)."
msgstr "Mirrors the image along the Y axis (top to bottom)."

# Thay Đổi Của Hình Ảnh -- Image Offset
msgid "Image Offset"
msgstr "Thay Đổi Của Hình Ảnh"

msgid "Used to move the frames along the X and Y axis. Additionally it disables the auto-scaling of the image."
msgstr "Used to move the frames along the X and Y axis. Additionally it disables the auto-scaling of the image."

# Cắt Xén Hình Ảnh -- Image Crop
msgid "Image Crop"
msgstr "Cắt Xén Hình Ảnh"

msgid "Used to crop the source image, use *Top*, *Left*, *Bottom*, and *Right* to control which part of the image is cropped."
msgstr "Used to crop the source image, use *Trên/Đỉnh -- Top*, *Trái -- Left*, *Dưới/Đáy -- Bottom*, and *Phải -- Right* to control which part of the image is cropped."

# Phim Video -- Video
msgid "Video"
msgstr "Phim Video"

# Chớp/Giật -- Strobe
msgid "Strobe"
msgstr "Chớp/Giật"

msgid "To only display each nth frame. For example, if you set this to 10, the strip will only display frames 1, 11, 21, 31, 41... of the source. *Strobe* is a float value -- this way you can get a strobe effect synced exactly to a beat, for example, by using non-integer values."
msgstr "To only display each nth frame. For example, if you set this to 10, the strip will only display frames 1, 11, 21, 31, 41... of the source. *Chớp/Giật -- Strobe* is a float value -- this way you can get a strobe effect synced exactly to a beat, for example, by using non-integer values."

msgid "Playback Direction"
msgstr "Playback Direction"

msgid "Options for choosing to play strip forwards or backwards."
msgstr "Options for choosing to play strip forwards or backwards."

# Màu Sắc -- Color
msgid "Color"
msgstr "Màu Sắc"

# Độ Bão Hòa -- Saturation
msgid "Saturation"
msgstr "Độ Bão Hòa"

msgid "Increase or decrease the saturation of an image."
msgstr "Increase or decrease the saturation of an image."

# Nhân -- Multiply
msgid "Multiply"
msgstr "Nhân"

msgid "Multiplies the colors by this value. This will increases the brightness."
msgstr "Multiplies the colors by this value. This will increases the brightness."

# Đổi Thành Số Thực Float -- Convert to Float
msgid "Convert to Float"
msgstr "Đổi Thành Số Thực Float"

msgid "Converts input to float data."
msgstr "Converts input to float data."

# Âm Thanh -- Sound
msgid "Sound"
msgstr "Âm Thanh"

# Âm Lượng -- Volume
msgid "Volume"
msgstr "Âm Lượng"

msgid "The volume of the sound. This value, even if animated will be reflected in the waveform."
msgstr "The volume of the sound. This value, even if animated will be reflected in the waveform."

# Cao Độ -- Pitch
msgid "Pitch"
msgstr "Cao Độ"

msgid "Coefficient of playback speed. This value will affect length of the strip, that will not be represented in the timeline."
msgstr "Coefficient of playback speed. This value will affect length of the strip, that will not be represented in the timeline."

# Lia -- Pan
msgid "Pan"
msgstr "Lia"

msgid "Used to pan the audio from left and right channels. Only works for mono sources. Values can be between -2 and 2, where 0 means front/center, -1 means to the left and 1 to the right. In case of multichannel audio (rear speakers) you can pan to those with the higher values: -2, 2 is back. So this value basically represents the angle at which it's played."
msgstr "Used to pan the audio from left and right channels. Only works for mono sources. Values can be between -2 and 2, where 0 means front/center, -1 means to the left and 1 to the right. In case of multichannel audio (rear speakers) you can pan to those with the higher values: -2, 2 is back. So this value basically represents the angle at which it's played."

# Hiển Thị Dạng Sóng -- Display Waveform
msgid "Display Waveform"
msgstr "Hiển Thị Dạng Sóng"

msgid "Display an approximate waveform of the sound file inside of the sound strip. The waveform reflects strip volume and it's animation using :doc:`keyframes </animation/keyframes/introduction>`."
msgstr "Display an approximate waveform of the sound file inside of the sound strip. The waveform reflects strip volume and it's animation using :doc:`số/khung khóa -- keyframes </animation/keyframes/introduction>`."

msgid "Mono"
msgstr "Mono"

msgid "Mixdown all audio channels into a single one."
msgstr "Mixdown all audio channels into a single one."

msgid "Time Panel"
msgstr "Time Panel"

msgid ":menuselection:`Sidebar region --> Strip --> Time`"
msgstr ":menuselection:`Sidebar region --> Strip --> Time`"

msgid "The Time panel is used to control source and timeline position of the strip."
msgstr "The Time panel is used to control source and timeline position of the strip."

# Khóa (biểu tượng cái khóa) -- Lock (padlock icon) -- Lock (padlock icon)
msgid "Lock (padlock icon)"
msgstr "Khóa (biểu tượng cái khóa) -- Lock (padlock icon)"

msgid "Prevents the strip from being moved (found in the panel header)."
msgstr "Prevents the strip from being moved (found in the panel header)."

# Kênh -- Channel
msgid "Channel"
msgstr "Kênh"

msgid "Changes the channel number, or row, of the strip."
msgstr "Changes the channel number, or row, of the strip."

# Khởi Đầu -- Start
msgid "Start"
msgstr "Khởi Đầu"

msgid "Changes the starting frame number of the strip, which is the same as selecting and moving the strip."
msgstr "Changes the starting frame number of the strip, which is the same as selecting and moving the strip."

# Độ Dài -- Duration
msgid "Duration"
msgstr "Độ Dài"

msgid "Changes the length, in frames of the strip. This works by changing the end frame, which is the same as selecting and moving the strip's right handle."
msgstr "Changes the length, in frames of the strip. This works by changing the end frame, which is the same as selecting and moving the strip's right handle."

# End
msgid "End"
msgstr "Kết Thúc"

msgid "Specifies the ending time and ending frame number for the strip."
msgstr "Specifies the ending time and ending frame number for the strip."

msgid "Strip Offset Start/End"
msgstr "Strip Offset Start/End"

msgid "Can be used to either extend the strip beyond the end frame by repeating the last frame. Or it can be used to shorten the strip, as if you were cropping the end frame. This is the same as adjusting the strip handles."
msgstr "Can be used to either extend the strip beyond the end frame by repeating the last frame. Or it can be used to shorten the strip, as if you were cropping the end frame. This is the same as adjusting the strip handles."

msgid "Hold Offset Start/End"
msgstr "Hold Offset Start/End"

msgid "Offset of the uncut strip content."
msgstr "Offset of the uncut strip content."

# Đầu Chơi -- Playhead
msgid "Playhead"
msgstr "Đầu Chơi"

msgid "Position of the Playhead relative to the start of the active strip."
msgstr "Position of the Playhead relative to the start of the active strip."

msgid "Source Panel"
msgstr "Source Panel"

msgid ":menuselection:`Sidebar region --> Strip --> Source`"
msgstr ":menuselection:`Sidebar region --> Strip --> Source`"

msgid "The Source panel is used to control sources of the strip such as filename and file path and various methods of interpreting these files."
msgstr "The Source panel is used to control sources of the strip such as filename and file path and various methods of interpreting these files."

# Đường/Đi/Dẫn -- Path
msgid "Path"
msgstr "Đường/Đi/Dẫn"

msgid "The directory that contains the source file. When the file is moved this can be updated instead of re-create the strip."
msgstr "The directory that contains the source file. When the file is moved this can be updated instead of re-create the strip."

# Tập Tin -- File
msgid "File"
msgstr "Tập Tin"

msgid "The file name of the source file. For image strips showing an image sequence, this will be different for each frame."
msgstr "The file name of the source file. For image strips showing an image sequence, this will be different for each frame."

# Change Data/Files
msgid "Change Data/Files"
msgstr "Đổi Dữ Liệu/Tập Tin"

msgid "Same as the *Path* and *File* fields, but this time combined to open the File Browser in order to find the file(s) you search. Same as :menuselection:`Strip --> Inputs --> Change Paths/Files`."
msgstr "Same as the *Đường/Đi/Dẫn -- Path* and *Tập Tin -- File* fields, but this time combined to open the File Browser in order to find the file(s) you search. Same as :menuselection:`Strip --> Inputs --> Change Paths/Files`."

# MPEG Preseek
msgid "MPEG Preseek"
msgstr "MPEG Tìm Trước"

msgid "Movie strip only -- Use Preseek field to tell Blender to look backward and compose an image based on the specified amount of previous frames (e.g. 15 for MPEG-2 DVD)."
msgstr "Movie strip only -- Use Preseek field to tell Blender to look backward and compose an image based on the specified amount of previous frames (e.g. 15 for MPEG-2 DVD)."

# Không Gian Màu Sắc -- Color Space
msgid "Color Space"
msgstr "Không Gian Màu Sắc"

msgid "To specify the color space of the source file."
msgstr "To specify the color space of the source file."

# Chế Độ Alpha -- Alpha mode
msgid "Alpha mode"
msgstr "Chế Độ Alpha"

msgid "If the source file has an Alpha (transparency) channel, you can choose:"
msgstr "If the source file has an Alpha (transparency) channel, you can choose:"

msgid ":term:`Straight Alpha` or :term:`Premultiplied Alpha`"
msgstr ":term:`Alpha Thẳng -- Straight Alpha` or :term:`Alpha Nhân Trước -- Premultiplied Alpha`"

# Chỉ Số của Luồng -- Stream index
msgid "Stream index"
msgstr "Chỉ Số của Luồng"

msgid "Movie strip only -- For files with several movie streams, use the stream with the given index."
msgstr "Movie strip only -- For files with several movie streams, use the stream with the given index."

# Gộp Trường -- Deinterlace
msgid "Deinterlace"
msgstr "Gộp Trường"

msgid "Removes fields in a video file. For example, if it is an analog video and it has even or odd interlacing fields."
msgstr "Removes fields in a video file. For example, if it is an analog video and it has even or odd interlacing fields."

# Độ Phân Giải -- Resolution
msgid "Resolution"
msgstr "Độ Phân Giải"

msgid "Resolution of the active strip image output."
msgstr "Resolution of the active strip image output."

msgid "Options for Sound Strips"
msgstr "Options for Sound Strips"

msgid ":ref:`Data-block menu <ui-data-block>` to select a sound."
msgstr ":ref:`Trình Đơn Khối Dữ Liệu -- Data-block menu <ui-data-block>` to select a sound."

msgid "Path to the sound file used by this :ref:`data-block <ui-data-block>` menu."
msgstr "Path to the sound file used by this :ref:`khối dữ liệu -- data-blocks -- data-block <ui-data-block>` menu."

# Đóng Gói -- Pack
msgid "Pack"
msgstr "Đóng Gói"

msgid "Pack sound into the blend-file."
msgstr "Pack sound into the blend-file."

# Giữ Trong Bộ Nhớ Đệm -- Caching
msgid "Caching"
msgstr "Giữ Trong Bộ Nhớ Đệm"

msgid "Sound file is decoded and loaded into the RAM."
msgstr "Sound file is decoded and loaded into the RAM."

