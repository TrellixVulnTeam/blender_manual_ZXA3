msgid "Transform"
msgstr "Biến Hóa -- Transform"

msgid "Transform is a swiss-army knife of image manipulation. It moves, rotates, and scales the images within a strip."
msgstr "Biến Hóa là a swiss-army dao của hình ảnh manipulation. It di chuyển, rotates, và scales the hình ảnh within a dải."

msgid "Options"
msgstr "Tùy Chọn -- Options"

msgid "Interpolation"
msgstr "Nội Suy -- Interpolation"

msgid "None"
msgstr "Không Có -- None"

msgid "No interpolation, uses nearest neighboring pixel."
msgstr "Không có nội suy, sử dụng điểm ảnh lân cận nhất."

msgid "Bilinear"
msgstr "Song Tuyến Tính -- Bilinear"

msgid "Simple interpolation between adjacent pixels."
msgstr "Nội suy đơn giản giữa các điểm ảnh lân cận."

msgid "Bicubic"
msgstr "Song Lập Phương -- Bicubic"

msgid "Highest quality interpolation."
msgstr "Nội suy chất lượng cao nhất."

msgid "Translation Unit"
msgstr "Đơn Vị Dịch Chuyển -- Translation Unit"

msgid "Control whether the input values are in *Percent* or *Pixels*."
msgstr "Điều Khiển whether the đầu vào giá trị là vào *Phần Trăm -- Percent* hoặc *Điểm Ảnh -- Pixels*."

msgid "Position"
msgstr "Vị Trí -- Position"

msgid "Moves the input along the X and Y axis."
msgstr "Di Chuyển the đầu vào dọc theo the X và Y trục."

msgid "Uniform Scale"
msgstr "Tỷ Lệ Đồng Đều -- Uniform Scale"

msgid "Scale the input evenly along the X and Y axis."
msgstr "Tỷ Lệ the đầu vào evenly dọc theo the X và Y trục."

msgid "Scale"
msgstr "Tỷ Lệ -- Scale"

msgid "Scale the image on the X and Y axis."
msgstr "Tỷ Lệ the hình ảnh bật the X và Y trục."

msgid "Rotation"
msgstr "Xoay Chiều -- Rotation"

msgid "Rotates the input two-dimensionally along the Z axis."
msgstr "Rotates the đầu vào two-dimensionally dọc theo the Z trục."

msgid "Example"
msgstr "Ví Dụ -- Example"

msgid "Transform Effect."
msgstr "Biến Hóa Hiệu Ứng."

