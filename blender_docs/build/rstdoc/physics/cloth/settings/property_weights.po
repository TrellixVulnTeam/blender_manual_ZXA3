# Trọng Lượng Của Tính Chất -- Property Weights
msgid "Property Weights"
msgstr "Trọng Lượng Của Tính Chất"

# Tham Chiếu -- Reference
msgid "Reference"
msgstr "Tham Chiếu"

msgid "Panel"
msgstr "Bảng"

msgid ":menuselection:`Physics --> Cloth --> Property Weights`"
msgstr ":menuselection:`Physics --> Cloth --> Property Weights`"

msgid "This panel is used to constrain certain cloth properties to a certain Vertex Group. The properties that they control can be found in a combination of the *Physical Properties* and Shape panels."
msgstr "This panel is used to constrain certain cloth properties to a certain Vertex Group. The properties that they control can be found in a combination of the *Tính Chất Vật Lý -- Physical Properties* and Shape panels."

# Nhóm Cấu Trúc -- Structural Group
msgid "Structural Group"
msgstr "Nhóm Cấu Trúc"

msgid "Defines a Vertex Group to control over structural stiffness."
msgstr "Defines a Vertex Group to control over structural stiffness."

# Mức Canhg Tối Đa -- Max Tension
msgid "Max Tension"
msgstr "Mức Canhg Tối Đa"

msgid "Maximum tension stiffness value."
msgstr "Maximum tension stiffness value."

# Mức Nén Tối Đa -- Max Compression
msgid "Max Compression"
msgstr "Mức Nén Tối Đa"

msgid "Maximum Compression stiffness value."
msgstr "Maximum Compression stiffness value."

# Nhóm Về Mức Xiêu Vẹo -- Shear Group
msgid "Shear Group"
msgstr "Nhóm Về Mức Xiêu Vẹo"

msgid "Vertex Group for fine control over shear stiffness."
msgstr "Vertex Group for fine control over shear stiffness."

# Mức Xiêu Vẹo Tối Đa -- Max Shearing
msgid "Max Shearing"
msgstr "Mức Xiêu Vẹo Tối Đa"

msgid "Maximum shear scaling value."
msgstr "Maximum shear scaling value."

# Nhóm Uốn Cong -- Bending Group
msgid "Bending Group"
msgstr "Nhóm Uốn Cong"

msgid "Vertex Group for fine control over bending stiffness."
msgstr "Vertex Group for fine control over bending stiffness."

# Mức Uốn Cong Tối Đa -- Max Bending
msgid "Max Bending"
msgstr "Mức Uốn Cong Tối Đa"

msgid "Maximum bending stiffness value."
msgstr "Maximum bending stiffness value."

# Nhóm Về Mức Co Lại -- Shrinking Group
msgid "Shrinking Group"
msgstr "Nhóm Về Mức Co Lại"

msgid "Vertex Group for shrinking cloth."
msgstr "Vertex Group for shrinking cloth."

# Mức Co Lại Tối Đa -- Max Shrinking
msgid "Max Shrinking"
msgstr "Mức Co Lại Tối Đa"

msgid "Max amount to shrink cloth by."
msgstr "Max amount to shrink cloth by."

