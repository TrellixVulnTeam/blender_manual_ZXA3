# Phổ Thông -- Generic
msgid "Generic"
msgstr "Phổ Thông"

# Tham Chiếu -- Reference
msgid "Reference"
msgstr "Tham Chiếu"

msgid "Panel"
msgstr "Bảng"

# :menuselection:`Physics --> Rigid Body Constraint`
msgid ":menuselection:`Physics --> Rigid Body Constraint`"
msgstr ":menuselection:`Vật Lý (Physics) --> Ràng Buộc Thân Cứng (Rigid Body Constraint)`"

msgid "Type"
msgstr "Thể Loại"

msgid "The generic constraint has a lot of available parameters."
msgstr "The generic constraint has a lot of available parameters."

msgid "The X, Y, and Z axis constraints can be used to limit the amount of translation between the objects. Clamping the min/max to zero has the same effect as the Point constraint."
msgstr "The X, Y, and Z axis constraints can be used to limit the amount of translation between the objects. Clamping the min/max to zero has the same effect as the Point constraint."

msgid "Clamping the relative rotation to zero keeps the objects in alignment. Combining an absolute rotation and translation clamp would behave much like the Fixed constraint."
msgstr "Clamping the relative rotation to zero keeps the objects in alignment. Combining an absolute rotation and translation clamp would behave much like the Fixed constraint."

msgid "Using a non-zero spread on any parameter allows it to rattle around in that range throughout the course of the simulation."
msgstr "Using a non-zero spread on any parameter allows it to rattle around in that range throughout the course of the simulation."

# Tùy Chọn -- Options
msgid "Options"
msgstr "Tùy Chọn"

# Giới Hạn -- Limits
msgid "Limits"
msgstr "Giới Hạn"

msgid "X Axis/Y Axis/Z axis"
msgstr "X Axis/Y Axis/Z axis"

msgid "Enables/disables limit translation on X, Y or Z axis respectively."
msgstr "Enables/disables limit translation on X, Y or Z axis respectively."

# Thường/Dưới -- Lower
msgid "Lower"
msgstr "Thường/Dưới"

msgid "Lower limit of translation for X, Y or Z axis respectively."
msgstr "Lower limit of translation for X, Y or Z axis respectively."

# Chữ Hoa/Bên Trên -- Upper
msgid "Upper"
msgstr "Chữ Hoa/Bên Trên"

msgid "Upper limit of translation for X, Y or Z axis respectively."
msgstr "Upper limit of translation for X, Y or Z axis respectively."

msgid "X Angle/Y Angle/Z Angle"
msgstr "X Angle/Y Angle/Z Angle"

msgid "Enables/disables limit rotation around X, Y or Z axis respectively."
msgstr "Enables/disables limit rotation around X, Y or Z axis respectively."

msgid "Lower limit of rotation for X, Y or Z axis respectively."
msgstr "Lower limit of rotation for X, Y or Z axis respectively."

msgid "Upper limit of rotation for X, Y or Z axis respectively."
msgstr "Upper limit of rotation for X, Y or Z axis respectively."

