msgstr "Enable time profiling for background jobs."

msgid "``--debug-python``"
msgstr "``--debug-python``"

msgid "Enable debug messages for Python."
msgstr "Enable debug messages for Python."

msgid "``--debug-depsgraph``"
msgstr "``--debug-depsgraph``"

