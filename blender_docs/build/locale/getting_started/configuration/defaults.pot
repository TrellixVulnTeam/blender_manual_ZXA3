msgstr ""

#: /Users/hoangduytran/blender_docs/manual/getting_started/configuration/defaults.rst:28
msgid "Shortcuts"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/getting_started/configuration/defaults.rst:16
msgid "Presets for the default :doc:`keymap </editors/preferences/keymap>` for Blender. Note that this manual assumes that you use the \"Blender\" keymap."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/getting_started/configuration/defaults.rst:20
msgid "Blender"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/getting_started/configuration/defaults.rst:20
