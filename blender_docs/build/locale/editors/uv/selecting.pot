msgid "This selector lets you enable automatic additional selection."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/editors/uv/selecting.rst:73
msgid "Shared Vertex"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/editors/uv/selecting.rst:74
msgid "Selects UVs that share a mesh vertex, even if they are in different UV locations."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/editors/uv/selecting.rst:75
msgid "Shared Location"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/editors/uv/selecting.rst:76
msgid "Selects UVs that are in the same UV location and share a mesh vertex."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/editors/uv/selecting.rst:80
msgid "Disabled"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/editors/uv/selecting.rst:78
msgid "Disables Sticky Selection. When you move a UV in this mode, each face owns its own UVs, allowing them to be separated."
msgstr ""

