msgstr ""

#: /Users/hoangduytran/blender_docs/manual/editors/nla/properties_modifiers.rst:29
msgid "Hold Forward"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/editors/nla/properties_modifiers.rst:30
