msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:20
msgid "Start"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:20
msgid "Start distance of the volumetric effect."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:23
msgid "End"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:23
msgid "End distance of the volumetric effect."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:27
msgid "Tile Size"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:26
msgid "Controls the quality of the volumetric effects. Lower size increases video memory usage and quality. This is the size in pixels of a volumetric cell."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:31
msgid "Samples"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:30
msgid "Number of samples to compute volumetric effects. Higher count increases video memory usage and quality. These samples are distributed along the view depth (view Z axis)."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:35
msgid "Distribution"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:34
msgid "Blend between linear and exponential sample distribution. Higher values puts more samples near the camera."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:40
msgid "Volumetric Lighting"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:42
msgid "Let the volume scattering scatter light in the scene. Unnecessary if no Volume Scatter is present in the scene."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:48
msgid "Light Clamping"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:46
msgid "Clamp light contribution of the volume scattering effect. Reduces flickering and noise. Set to 0.0 to disable clamping."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:53
msgid "Volumetric Shadows"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:55
msgid "Approximate light absorption of the surrounding volume objects. This makes the volumes more opaque to light. This is a very expensive option and has limitations (link)."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:59
msgid "Shadow Samples"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/render/eevee/render_settings/volumetrics.rst:59
msgid "Number of samples to compute volumetric shadowing."
msgstr ""

