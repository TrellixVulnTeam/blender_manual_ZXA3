#: ../../manual/animation/constraints/transform/maintain_volume.rst:5
msgid "Maintain Volume Constraint"
msgstr ""

#: ../../manual/animation/constraints/transform/maintain_volume.rst:7
msgid "The *Maintain Volume* constraint limits the volume of a mesh or a bone to a given ratio of its original volume."
msgstr ""

#: ../../manual/animation/constraints/transform/maintain_volume.rst:12
msgid "Options"
msgstr ""

#: ../../manual/animation/constraints/transform/maintain_volume.rst:21
msgid "Free"
msgstr ""

#: ../../manual/animation/constraints/transform/maintain_volume.rst:20
msgid "The free-scaling axis of the object."
msgstr ""

#: ../../manual/animation/constraints/transform/maintain_volume.rst:22
msgid "X, Y, Z"
msgstr ""

#: ../../manual/animation/constraints/transform/maintain_volume.rst:33
msgid "Mode"
msgstr ""

#: ../../manual/animation/constraints/transform/maintain_volume.rst:24
msgid "Specifies how the constraint handles scaling of the non-free axes."
msgstr ""

#: ../../manual/animation/constraints/transform/maintain_volume.rst:27
msgid "Strict"
msgstr ""

#: ../../manual/animation/constraints/transform/maintain_volume.rst:27
msgid "This mode overrides non-free axis scaling to strictly maintain the specified volume. Only the ratio between the scale of the non-free axes is passed through."
msgstr ""

#: ../../manual/animation/constraints/transform/maintain_volume.rst:30
msgid "Uniform"
msgstr ""

#: ../../manual/animation/constraints/transform/maintain_volume.rst:30
msgid "This mode maintains the volume as specified only when the pre-constraint scaling is uniform. Deviations from uniform scaling on non-free axes are passed through."
msgstr ""

#: ../../manual/animation/constraints/transform/maintain_volume.rst:33
msgid "Single Axis"
msgstr ""

#: ../../manual/animation/constraints/transform/maintain_volume.rst:33
msgid "This mode maintains the volume only when the object is scaled just on its free axis. Any additional non-free axis scaling is passed through."
msgstr ""

#: ../../manual/animation/constraints/transform/maintain_volume.rst:35
msgid "Volume"
msgstr ""

#: ../../manual/animation/constraints/transform/maintain_volume.rst:36
msgid "The bone's rest volume."
msgstr ""

#: ../../manual/animation/constraints/transform/maintain_volume.rst:38
msgid "Space"
msgstr ""

#: ../../manual/animation/constraints/transform/maintain_volume.rst:38
msgid "This constraint allows you to choose in which space to evaluate its owner's transform properties."
msgstr ""

