msgstr ""

#: /Users/hoangduytran/blender_docs/manual/interface/window_system/status_bar.rst:38
msgid "Resource Information"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/interface/window_system/status_bar.rst:40
