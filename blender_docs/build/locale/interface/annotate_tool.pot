msgstr ""

#: /Users/hoangduytran/blender_docs/manual/interface/annotate_tool.rst:35
msgid "Color"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/interface/annotate_tool.rst:35
msgid "Adjusts the color of existing and new strokes."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/interface/annotate_tool.rst:38
msgid "Thickness"
msgstr ""

