msgid "Layers adjustment panel."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/grease_pencil/properties/layers.rst:124
msgid "Tint Color/Factor"
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/grease_pencil/properties/layers.rst:123
