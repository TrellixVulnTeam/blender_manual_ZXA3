msgstr ""

#: /Users/hoangduytran/blender_docs/manual/movie_clip/masking/selecting_editing.rst:11
msgid "Toggle Select All :kbd:`A`."
msgstr ""

#: /Users/hoangduytran/blender_docs/manual/movie_clip/masking/selecting_editing.rst:12
msgid "Box and Circle Select :kbd:`B`, :kbd:`C`."
msgstr ""

